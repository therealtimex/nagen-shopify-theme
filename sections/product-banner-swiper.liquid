{% render 'section-style' %}
{%- assign id = '#shopify-section-' | append: section.id -%}

{% liquid
  assign section_header_space = section.settings.section_header_mb

  assign section_title_size = section.settings.title_size
  assign section_title_height = section.settings.title_height
  assign section_title_space = section.settings.title_mb

  assign link_font_size = section.settings.link_font_size | append: 'px'
  assign link_line_height = section.settings.link_line_height
    assign link_color = section.settings.link_color
    assign link_color_hover = section.settings.link_color_hover
%}

<style>
  {% if section.settings.nowrap %}
    #shopify-section-{{ section.id }}
    .swiper-container {
      clip-path: unset;
    }
  {% endif %}

  {{ id }} .product__banner--wrapper {
    padding: {{ section.settings.content_padding }};
    background-color: {{ section.settings.content_color }};
    border-radius: 5px;
  }

  {{ id }}
  .section__header {
    margin: 0 0 {{ section_header_space }}px;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
  }

  {{ id }} .section__header--countdown {
    display: inline-flex;
    align-items: center;
    padding: 9px 17px;
    border-radius: 100px;
    background-color: var(--primary_hover);
  }

  {{ id }}  .header-countdown-title {
    margin-right: 7px;
  }

  {{ id }} #countdowntimer .word {
    display: none;
  }

  {{ id }} #countdowntimer>div.sec {
    display: none;
  }

  {{ id }}
  .section__header .title {
    margin: 0 0 {{ section_title_space }}px;
    font-size: {{ section_title_size }}px;
    line-height: {{ section_title_height }}%;
  }



  {{ id }} .product__banner {
    width: {{ section.settings.banner_width }};
    flex: none;
    position: relative;
    max-width: 50%;
  }
  {{ id }} .product__swiper {
    width: calc(100% - {{ section.settings.banner_width }});
  }


  {{ id }} .swiper-button-next, {{ id }} .swiper-button-prev {
    position: initial;
    transform: none;
  }

  {{ id }} .swiper-arrow {
    display: flex;
    align-items: center;
    gap: 10px;

    position: absolute;
    top: -56px;
    right: 15px;
  }

  {{ id }} .product__banner--content {
    position: absolute;
    top: 32px;
    left: 41px;
    right: 41px;
  }

  {{ id }} .product__banner--links {
    line-height: 1;
  }

{{ id }} .product__banner--links a {
    color: {{ link_color }};
    font-size: {{ link_font_size }};
    line-height: {{ link_line_height }};
    font-family: var(--g-font-other)
}

{{ id }} .product__banner--links a:hover {
    color: {{ link_color_hover }};
}

{{ id }} .product__banner--bg {
    height: 100%;
    border-radius: 5px;
    overflow: hidden;
}

{{ id }} .product__banner--bg img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

  @media (min-width: 1500px) {
    #shopify-section-{{ section.id }} .countdown-header.text-center {
      max-width: 350px;
    }
    .text-w-product {
      display: flex;
      align-items: start;
      gap: 55px;
    }
    .text-w-product .section__header-countdown {
      width: 25%;
    }
    .text-w-product .product-sw-list {
      width: 75%;
    }
  }

  @media (max-width: 575px) {
    {{ id }} .product__banner--wrapper {
        padding: 20px;
    }

    {{ id }} .product__banner--content {
        inset: 20px;
    }
  }

  @media (max-width: 479px) {
    {{ id }} .product__banner {
        width: 100%;
        max-width: 100%;
    }

    {{ id }} .product__swiper {
        width: 100%;
    }

    {{ id }} .swiper-arrow {
        display: none;
    }
  }
</style>

<section class="section-{{ section.id }}">
  {% unless section.settings.setwidthout == 'full' %}
    <div class="{{ section.settings.setwidthout }}">
  {% endunless %}

  <div class="product__banner--wrapper">

  {% render 'section-header' %}

  {%- liquid
    assign banner_column_desktop = section.settings.banner_column_desktop | default: 0
    assign product_column_desktop = 12 | minus: banner_column_desktop

    assign banner_column_tablet = section.settings.banner_column_tablet | default: 0
    assign product_column_tablet = 12 | minus: banner_column_tablet

    assign banner_column_tablet_small = section.settings.banner_column_tablet_small | default: 0
    assign product_column_tablet_small = 12 | minus: banner_column_tablet_small
  -%}

  <div class="row">
    
        {%- for block in section.blocks -%}
            {%- case block.type -%}
                {%- when 'banner' -%}
                    <div class="product__banner col" {% if section.settings.banner_data_aos !="none" %}
                        data-aos="{{ section.settings.banner_data_aos }}"
                        data-aos-offset="-50" {% endif %}>
                        <div class="product__banner--bg">
                            {{ block.settings.image |  image_url: width: block.settings.image.width |  image_tag }}
                        </div>
                        <div class="product__banner--content">
                            <ul class="product__banner--links">
                            {%- for link in block.settings.block_link.links -%}
                                <li>
                                    <a href="{{ link.url }}" class="d-inline-flex align-items-center">
                                        <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M12.172 12L9.34302 9.17202L10.757 7.75702L15 12L10.757 16.243L9.34302 14.828L12.172 12Z"
                                            fill="currentColor" />
                                    </svg>   
                                    {{ link.title }}</a>
                                </li>
                            {%- endfor -%}
                            </ul>
                        </div>
                    </div>
                {%- endcase -%}
            
        {%- endfor -%}
    
    
        <div class="product__swiper position-relative col">
            <div
              id="swiper-{{ section.id }}"
              class="ap-swiper swiper"
              data-desktopbig-item="{{ section.settings.desktop_big_item }}"
              data-desktop-item="{{ section.settings.desktop_item }}"
              data-laptop-item="{{ section.settings.laptop_item }}"
              data-tablet-item="{{ section.settings.tablet_item }}"
              data-tabletsmall-item="{{ section.settings.tablet_small_item }}"
              data-mobile-item="{{ section.settings.mobile_item }}"
              data-loop="{{ section.settings.loop }}"
              data-rows="{{ section.settings.rows }}"
              data-direction="{{ section.settings.direction }}"
              data-navigation="{{ section.settings.navigation }}"
              data-pagination="{{ section.settings.pagination }}"
              data-space="{{ section.settings.space }}"
              data-space-desktop="{{ section.settings.space_desktop }}"
              data-time="{{ section.settings.time }}">
              <div class="swiper-wrapper">
                {% liquid
                  if section.settings.collection != blank
                    assign limit = section.settings.limit
                    for product in section.settings.collection.products limit: limit
                      echo '<div class="swiper-slide">'
                      render 'product-grid', product: product, data_aos: section.settings.product_data_aos
                      echo '</div>'
                    endfor
                  else
                    for i in (1..6)
                      echo '<div class="swiper-slide">'
                      capture current
                        cycle 1, 2, 3, 4, 5, 6
                      endcapture
                      assign img = 'product-' | append: current | placeholder_svg_tag: 'placeholder-svg'
                      echo img
                      echo '</div>'
                    endfor
                  endif
                %}
        
        
              </div>
        
              
        
              {% if section.settings.pagination %}
                <div class="swiper-pagination swiper-pagination_{{ section.id }}"></div>
              {% endif %}
            </div>
            {% if section.settings.navigation %}
              <div class="swiper-arrow">
                <div class="swiper-button-prev swiper-button-prev_{{ section.id }}">
                    <svg width="11" height="10" viewBox="0 0 11 10" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path
                        d="M0.757812 5.34375L4.32031 9.09375C4.50781 9.28125 4.73438 9.375 5 9.375C5.25 9.375 5.46875 9.28906 5.65625 9.11719C5.84375 8.94531 5.9375 8.72656 5.9375 8.46094C5.9375 8.19531 5.85156 7.96875 5.67969 7.78125L3.61719 5.625H10.0625C10.3281 5.625 10.5547 5.53906 10.7422 5.36719C10.9141 5.17969 11 4.95312 11 4.6875C11 4.42188 10.9141 4.20312 10.7422 4.03125C10.5547 3.84375 10.3281 3.75 10.0625 3.75H3.61719L5.67969 1.59375C5.85156 1.40625 5.9375 1.17969 5.9375 0.914062C5.9375 0.648438 5.84375 0.429688 5.65625 0.257812C5.46875 0.0859375 5.24219 0 4.97656 0C4.71094 0 4.49219 0.09375 4.32031 0.28125L0.757812 4.03125C0.585938 4.21875 0.5 4.4375 0.5 4.6875C0.5 4.9375 0.585938 5.15625 0.757812 5.34375Z"
                        fill="currentColor" />
                </svg>
                </div>
                <div class="swiper-button-next swiper-button-next_{{ section.id }}">
                    <svg width="11" height="10" viewBox="0 0 11 10" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M10.2422 5.34375L6.67969 9.09375C6.49219 9.28125 6.26562 9.375 6 9.375C5.75 9.375 5.53125 9.28906 5.34375 9.11719C5.15625 8.94531 5.0625 8.72656 5.0625 8.46094C5.0625 8.19531 5.14844 7.96875 5.32031 7.78125L7.38281 5.625H0.9375C0.671875 5.625 0.445312 5.53906 0.257812 5.36719C0.0859375 5.17969 0 4.95312 0 4.6875C0 4.42188 0.0859375 4.20312 0.257812 4.03125C0.445312 3.84375 0.671875 3.75 0.9375 3.75H7.38281L5.32031 1.59375C5.14844 1.40625 5.0625 1.17969 5.0625 0.914062C5.0625 0.648438 5.15625 0.429688 5.34375 0.257812C5.53125 0.0859375 5.75781 0 6.02344 0C6.28906 0 6.50781 0.09375 6.67969 0.28125L10.2422 4.03125C10.4141 4.21875 10.5 4.4375 10.5 4.6875C10.5 4.9375 10.4141 5.15625 10.2422 5.34375Z" fill="currentColor"/>
                    </svg>
                    
                </div>
        
              </div>
            {% endif %}
          </div>
    
    
  </div>

  


  
  </div>

  {% unless section.settings.setwidthout == 'full' %}
    </div>
  {% endunless %}
</section>

