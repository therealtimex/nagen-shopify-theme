{% render 'section-style' %}

{%- assign id = '#shopify-section-' | append: section.id -%}

<style>

  {{ id }} {
      {%- if section.settings.background_repeat -%}
        background-repeat: repeat;
      {%- else -%}
        background-repeat: no-repeat;
      {%- endif -%}
      {%- if section.settings.background_size != "default" and section.settings.background_size != "custom" -%}
        background-size: {{ section.settings.background_size }};
      {%- endif -%}
      {%- if section.settings.background_size == "custom" -%}
        background-size: {{ section.settings.background_size_custom }}%;
      {%- endif -%}
      {%- if section.settings.background_position != "custom" -%}
        background-position: {{ section.settings.background_position }};
      {%- else -%}
        background-position-x: {{ section.settings.background_position_x }};
        background-position-y: {{ section.settings.background_position_y }};
      {%- endif -%}
    }


{{ id }} .tabs__nav {
  width: 33.33%;
  flex: none;
}

{{ id }} .tabs__content {
  width: 66.67%;
  flex: none;
}

{{ id }} .ap-navtabs {
  margin-bottom: 0;
}

{{ id }} .ap-navtabs__position {
  display: none;
}

{{ id }} .ap-navtabs__item-list {
  max-width: 350px;
}

{{ id }} .ap-navtabs__item {
  font-size: 32px;
  line-height: 1.25;
  font-weight: 500;
  color: #fff;
  padding: 0 0 20px;
  position: relative;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

{{ id }} .ap-navtabs__item span {
  transition: 0.3s all ease;
  opacity: 0;
  visibility: hidden;
  transform: translateX(-30px);
}

{{ id }} .ap-navtabs__item::before {
  content: '';
  position: absolute;
  bottom: 0;
  left: auto;
  right: 0;
  width: 100%;
  height: 1px;
  background-color: #fff;
  opacity: 0.1;
  transition: 0.3s all ease;
}
{{ id }} .ap-navtabs__item::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: auto;
  width: 0;
  height: 1px;
  background-color: #fff;
  transition: 0.3s all ease;
}

{{ id }} .ap-navtabs__item[ap-expanded-aria="true"]::before {
  width: 0;
}

{{ id }} .ap-navtabs__item[ap-expanded-aria="true"]::after {
  width: 100%;
}

{{ id }} .ap-navtabs__item[ap-expanded-aria="true"] span {
  opacity: 1;
  visibility: visible;
  transform: none;
}

{{ id }} .ap-navtabs__item:not(:last-child) {
  margin: 0 0 20px;
}

{{ id }} .product-tabs__tab-item-wrapper {
  border: none;
  padding: 0;
}

{{ id }} .collapsible {
  display: flex;
  flex-wrap: wrap;
  flex-direction: row-reverse;
  background-color: #fff;
  border-radius: 20px;
  padding: 0;
  visibility: visible;
  height: auto;
}

{{ id }} .collapsible-text {
  width: calc(50% - 15px);
  padding: 40px;
}
{{ id }} .collapsible-img {
  width: calc(50% + 15px);
}

{{ id }} .collapsible-img img {
  height: 100%;
  border-radius: 20px;
  object-fit: cover;
}

{{id}} .collapsible-text-wp {
  display: flex;
  flex-direction: column;
  height: 100%;
}

{{ id }} .contentaps-title {
  flex: 1;
  margin-bottom: 20px;
  padding-bottom: 20px;
  font-size: 24px;
  line-height: calc(32/24);
  font-weight: 500;
  border-bottom: 1px solid #E7E2E2;
  color: #000;
}

{{ id }} .tab__content--icon {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 20px;
  line-height: 28px;
  font-weight: 500;
  color: #999999;
  margin-bottom: 10px;
}

{{ id }} .collapsible-description {
  margin-bottom: 30px;
}

{{ id }} .btn-secondary {
  color: #000;
}

{{ id }} .ap-navtabs[arrows] .ap-navtabs__scroller.is-scrollable:before {
  display: none;
}

@media (max-width: 991px) {
  {{ id }} .ap-navtabs__item {
    font-size: 24px;
  }
  {{ id }} .collapsible-text {
    padding: 24px;
    width: 50%;
  }
  
  {{ id }} .collapsible-img {
    width: 50%;
  }
}

@media (max-width: 767px) {
  {{ id }} .tabs__nav {
    width: 100%;
  }
  
  {{ id }} .tabs__content {
    width: 100%;
  }
  {{ id }} .ap-navtabs__item-list {
    max-width: 100%;
  }
  {{ id }} .ap-navtabs {
  margin-bottom: 30px;
}
{{ id }} .collapsible-text {
    min-width: 250px;
    flex: 1;
  }
  
  {{ id }} .collapsible-img {
    min-width: 200px;
    flex: 1;
  }

  {{ id }} .collapsible-img img {
  width: 100%;
}

}

@media (max-width: 575px) {
  {{ id }} .ap-navtabs__item:not(:last-child) {
    margin: 0 0 12px;
    padding: 0 0 12px;
    font-size: 20px;
  }
}

</style>

<section class="content-tabs">
    <div class="shopify-container">
      {% render 'section-header' %}

      <div class="row">
        
        <div class="col tabs__nav" {% if section.settings.data_aos_tab_nav !="none" %}
    data-aos="{{ section.settings.data_aos_tab_nav }}"{% endif %}>
    
            <ap-navtabs arrows="" class="ap-navtabs ap-navtabs--loose">
                <ap-scrollablecontent class="ap-navtabs__scroller hide-scrollbar">
                    <div class="ap-navtabs__scroller-inner">
                    <div class="ap-navtabs__item-list">
                        {% for block in section.blocks %}
                          {%- case block.type -%}
                            {%- when 'tab_custom' -%}
                              <button type="button" class="ap-navtabs__item heading h4 content-tabs-btn d-flex justify-content-between align-items-center" ap-expanded-aria="{% if forloop.index == 1 %}true{% else %}false{% endif %}" ap-controlsaria="block-{{ block.id }}">
                                {{ block.settings.tab_name }}
                                <span><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
<path fill-rule="evenodd" clip-rule="evenodd" d="M13.8675 11.5L7 18.2077L8.83495 20L16.62 12.3961C17.1267 11.9012 17.1267 11.0988 16.62 10.6039L8.83495 3L7 4.79225L13.8675 11.5Z" fill="currentColor"/>
</svg>
</span>
                              </button>

                          {%- endcase -%}
                        {% endfor %}
                    </div>
                    </div>
                </ap-scrollablecontent>
            </ap-navtabs>
    
        </div>
    
        <div class="col tabs__content" {% if section.settings.data_aos_tab_content !="none" %}
    data-aos="{{ section.settings.data_aos_tab_content }}"{% endif %}>
          {% for block in section.blocks %}

              <div id="block-{{ block.id }}" class="product-tabs__tab-item-wrapper" {% if forloop.index != 1 %}hidden{% endif %}>
    
                  {%- case block.type -%}
                      
                      {%- when 'tab_custom' -%}

                        <button is="toggle-button" class="collapsible-toggle heading heading--small hd-lap-and-up d-none" ap-controlsaria="block-{{ block.id }}-content" 
                        {% if forloop.index == 1 %}ap-expanded-aria="true"{% else %}ap-expanded-aria="false"{% endif %}>
                            {{ block.settings.tab_name }}
                        </button>
                        <ap-contentcollapsible id="block-{{ block.id }}-content" class="collapsible collapsible-wp" {% if forloop.index == 1 %}open{% endif %}>
                          <div class="collapsible-text">
                              <div class="collapsible-text-wp">
                                  {%- if block.settings.icon != blank -%}
                                    <div class="tab__content--icon">
                                      {{ block.settings.icon |  image_url: width: block.settings.icon.width |  image_tag }}

                                      <span>{%- if forloop.index < 10 -%}
                                        0
                                      {%- endif -%}{{ forloop.index }}</span>
                                    </div>
                                  {%- endif -%}
                                  <h3 class="contentaps-title">
                                      {{ block.settings.title }}
                                  </h3>
                                  {% if block.settings.tab_custom_content != blank %}
                                  <div class="collapsible-description">
                                    {{ block.settings.tab_custom_content }}
                                  </div>

                                  {%- if block.settings.btn_text != blank -%}
                                    <a class="btn-secondary" href="{{ blocks.settings.btn_link |  default: '#' }}">
                                      <div class="icon-button">{%- render 'icon-arrow' -%}</div>
                                      {{ block.settings.btn_text }}

                                    </a>
                                  {%- endif -%}

                                  
                                  {% endif %}
                              </div>
                          </div>
                          <div class="collapsible-img">
                            {% if block.settings.collapsible_img != blank %}
                              <img loading="lazy" 
                              sizes="(max-width: 740px) 80vw, (max-width: 999px) 60vw, 425px"
                              alt="" 
                              src="{{ block.settings.collapsible_img | image_url: width: block.settings.collapsible_img.width }}" 
                              data-srcset="{%- if block.settings.collapsible_img.width >= 3000 -%}{{ block.settings.collapsible_img | image_url: width: 3000, crop: 'center' }} 3000w,{%- endif -%}
                                  {{ block.settings.collapsible_img | image_url }} {{ block.settings.collapsible_img.width }}w"
                              src="{{ block.settings.collapsible_img | image_url: width: 1500 }}"
                              height="{{ block.settings.collapsible_img.height }}"
                              width="{{ block.settings.collapsible_img.width }}">
                            {% else %}
                              <div class="banner-list-image">
                              {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                              </div>
                            {% endif %}
                          </div>
                        </ap-contentcollapsible>
                      
                  {%- endcase -%}
              </div>
    
          {% endfor %}

          {% for block in section.blocks %}
            {%- case block.type -%}
              {%- when 'image-child' -%}
                <div class="contenttabs-imgchild">
                  {% if block.settings.img_child != blank %}
                    <img loading="lazy" 
                    sizes="(max-width: 740px) 80vw, (max-width: 999px) 60vw, 425px"
                    alt="" 
                    src="{{ block.settings.img_child | image_url: width: block.settings.img_child.width }}" 
                    data-srcset="{%- if block.settings.img_child.width >= 3000 -%}{{ block.settings.img_child | image_url: width: 3000, crop: 'center' }} 3000w,{%- endif -%}
                        {{ block.settings.img_child | image_url }} {{ block.settings.img_child.width }}w"
                    src="{{ block.settings.img_child | image_url: width: 1500 }}"
                    height="{{ block.settings.img_child.height }}"
                    width="{{ block.settings.img_child.width }}">
                  {% else %}
                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                  {% endif %}
                </div>
          
            {%- endcase -%}
          {% endfor %}

        </div>

      </div>

    </div>
</section>

