                                    <defs>
                                    <clipPath id="clip0_29_10">
                                    <rect width="24" height="24" fill="white"/>
                                    </clipPath>
                                    </defs>
                                  </svg>
                                </button>
                              </div>
                              {% endif %}
                          </div>
  
                          {%- when "payment_methods" -%}
                            
                            <div class="copyright__payment-methods copyright-item-{{ forloop.index }} text-{{ block.settings.text_align }}">
                              {% if block.settings.label != blank %}
                              <span class="copyright__payment-methods-label sub-heading mb-11">{{ block.settings.label }}</span>
                              {% endif %}
                              {%- if block.settings.payment_info == 'svg' -%}
                                {%- if shop.enabled_payment_types != empty -%}
                                  <ul id="footer-payment-methods" class="list-inline payment-icons text-center text-lg-center">
                                    {%- for type in shop.enabled_payment_types -%}
                                    <li class="list-inline-item">
                                      {{ type | payment_type_svg_tag: class: 'payment-icon' }}
                                    </li>
                                    {%- endfor -%}
                                  </ul>
                                {%- endif -%}
                              {%- else -%}
                                {%- if block.settings.payment_methods_image != blank -%}
                                  <img class="copyright__content-image" src="{{ block.settings.payment_methods_image | image_url: width: 400 }}" data-src="{{ block.settings.payment_methods_image | image_url: width: 400 }}"
                                    loading="lazy" alt=""
                                    width="{{ block.settings.payment_methods_image.width }}"
                                    height="{{ block.settings.payment_methods_image.height }}"/>
                                {%- endif -%}
                              {%- endif -%}
                            </div>
                      
                  {%- endcase -%}
                </div>
            {%- endfor -%}
           
          </div>
      </div>
    </div>
</section>
  
{% schema %}
{
  "name": "Copyright",
  "class": "shopify-section--copyright",
  "settings": [
    {
          "type": "checkbox",
          "id": "full_width",
          "label": "Full width",
          "default": false
        },
       {
        "type": "number",
        "id": "content_width",
        "label": "Content width",
        "default": 1290
       },
    {
      "type": "text",
      "id": "padding_desktop",
      "label": "t:sections.all.padding_desktop.label",
      "info": "t:sections.all.padding_desktop.info"
    },
    {
      "type": "text",
      "id": "padding_tablet",
      "label": "t:sections.all.padding_tablet.label",
      "info": "t:sections.all.padding_tablet.info"
    },
    {
      "type": "text",
      "id": "padding_mobile",
      "label": "t:sections.all.padding_mobile.label",
      "info": "t:sections.all.padding_mobile.info"
    },
    {
      "type": "image_picker",
      "id": "background_image",
      "label": "t:sections.all.background_image.label"
    },
    {
      "type": "color",
      "id": "background",
      "default": "#282828",
      "label": "t:sections.all.background.label"
    },
    {
      "type": "color",
      "id": "heading_color",
      "default": "#ffffff",
      "label": "t:sections.all.heading_color.label"
    },
    {
      "type": "color",
