{{ 'copyright.css' | asset_url | stylesheet_tag }}
{% render 'section-style' %}
{%- assign id = '#shopify-section-' | append: section.id -%}

<style>
    {{ id }} {
        --background: {{ section.settings.background | color_extract: 'red' }}, {{ section.settings.background | color_extract: 'green' }}, {{ section.settings.background | color_extract: 'blue' }};
        --heading-color: {{ section.settings.heading_color | color_extract: 'red' }}, {{ section.settings.heading_color | color_extract: 'green' }}, {{ section.settings.heading_color | color_extract: 'blue' }};
        --color-body: {{ section.settings.text_color | color_extract: 'red' }}, {{ section.settings.text_color | color_extract: 'green' }}, {{ section.settings.text_color | color_extract: 'blue' }};
        --color-body-2: {{ section.settings.text_color_2 }};
        --border-color: {{ section.settings.border_color }};
        --color-link : {{ section.settings.link_color | color_extract: 'red' }}, {{ section.settings.link_color | color_extract: 'green' }}, {{ section.settings.link_color | color_extract: 'blue' }};
        --color-social : {{ section.settings.color_social | color_extract: 'red' }}, {{ section.settings.color_social | color_extract: 'green' }}, {{ section.settings.color_social | color_extract: 'blue' }};
        --bg-link : {{ section.settings.bg_link | color_extract: 'red' }}, {{ section.settings.bg_link | color_extract: 'green' }}, {{ section.settings.bg_link | color_extract: 'blue' }};

        background-size: unset;
        --section-blocks-overlay-opacity: {{ section.settings.overlay_opacity | divided_by: 10.0 }};
    }

    {{ id }} {
      background-color: rgb(var(--background), var(--section-blocks-overlay-opacity));
    }
</style>
  
<section id="{{ section.id }}" class="copyright">
    <div class="shopify-container">
      <div class="copyright__inner">
        <div class="row align-items-center">
            {%- for block in section.blocks -%}
                <div id="block-{{ block.id }}" class="col-xl-{{ block.settings.extra_large_column }} col-lg-{{ block.settings.large_column }} col-md-{{ block.settings.medium_column }} col-sm-{{ block.settings.small_column }} col-12" >
                  {%- case block.type -%}
  
                      {%- when "logo" -%}
                        <style>
                          .logo-copyright.text-center {
                            margin: auto;
                          }
                          .logo-copyright.text-right {
                            margin-left: auto;
                          }
                        </style>
                      {%- if block.settings.logo != blank -%}
                      <div class="logo-copyright text-{{ block.settings.text_align }}">
                        <img style="width: {{ block.settings.logo }}px" loading="lazy"
                          sizes="{{ block.settings.logo }}px" class="footer__image" alt=""
                          src="{{ block.settings.logo | image_url: width: block.settings.logo.width }}"
                          data-srcset="{{ block.settings.logo | image_url: width: 100 }} 100w, {{ block.settings.logo | image_url: width: 200 }} 200w, {{ block.settings.logo | image_url: width: 300 }} 300w, {{ block.settings.logo | image_url: width: 400 }} 400w, {{ block.settings.logo | image_url: width: 500 }} 500w"
                          width="{{ block.settings.logo.width }}" height="{{ block.settings.logo.height }}">
                      </div>
                      {% endif %}
  
                      {%- when "copyright-text" -%}
                          <div class="text-md-{{ block.settings.text_align }} text-center">
                              {%- if block.settings.label != blank -%}
                                  <span class="copyright-label">{{ block.settings.label }}</span>
                              {%- endif -%}
                              {%- if block.settings.content != blank -%}
                                  <div class="copyright-item-content">
                                      {{ block.settings.content }}
                                  </div>
                              {%- endif -%}
                          </div>

                      {%- when "copyright-lc" -%}
                          <div style="gap: 8px;" class="copyright-lc-{{ forloop.index }} d-flex text-{{ block.settings.text_align }}">
                            {% if block.settings.enable_localization_currency %}
                              <div>
                                {% render 'localization-currency', id: 'HeaderCountryForm' %}
                              </div>
                            {% endif %}
                
                            {% if block.settings.enable_localization %}
                              <div>
                                {% render 'localization' %}
                              </div>
                            {% endif %}
                          </div>
                      
                      {%- when "menu" -%}
                          <style>
                            .text-center .linklist {
                              justify-content: center;
                            }
                            .text-right .linklist {
                              justify-content: end;
                            }
                          </style>
                          <div class="copyright-menu text-{{ block.settings.text_align }}">
                              {%- if block.settings.label != blank -%}
                                  <span class="copyright-label">{{ block.settings.label }}</span>
                              {%- endif -%}
                              {%- if block.settings.menu != blank -%}
                                  <div class="copyright-item-content">
                                      <ul class="linklist list--unstyled" role="list">
                                      {%- for link in block.settings.menu.links -%}
                                      <li class="linklist__item">
                                          <a href="{{ link.url }}" class="link--faded">{{ link.title | escape }}</a>
                                      </li>
                                      {% endfor %}
                                      </ul>
                                  </div>
                              {% endif %}
                          </div>
                      
                      {%- when "social" -%}
                          {% liquid
                              assign _show_social = false
                              if block.settings.social_facebook != blank
                                assign _show_social = true
                              endif
                              if block.settings.social_instagram != blank
                                assign _show_social = true
                              endif
                              if block.settings.social_pinterest != blank
                                assign _show_social = true
                              endif
                              if block.settings.social_youtube != blank
                                assign _show_social = true
                              endif
                              if block.settings.social_linkedin != blank
                                assign _show_social = true
                              endif
                          %}
                          <style>
                            .text-center .copyright-social {
                              justify-content: center;
                            }
                            .text-right .copyright-social {
                              justify-content: end;
                            }
                          </style>
                          <div class="copyright-social-wp text-{{ block.settings.text_align }}">
                              <div class="copyright-social">
                                {%- if block.settings.label != blank -%}
                                    <span class="copyright-label mr-2 pr-1">{{ block.settings.label }}</span>
                                {%- endif -%}
  
                                {%- if _show_social -%}
                                  <div class="copyright-item-content">
                                    <ul class="social-copyright social-media social-media--no-radius list--unstyled" role="list">
                                        {%- if block.settings.social_facebook != blank -%}
                                          <li class="social-media__item social-media__item--facebook">
                                            <a href="{{ block.settings.social_facebook }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Facebook">
                                              <svg focusable="false" width="9" height="17" class="icon icon--facebook   " viewBox="0 0 9 17">
                                              <path fill-rule="evenodd" clip-rule="evenodd" d="M2.486 16.2084L2.486 8.81845H0L0 5.93845L2.486 5.93845L2.486 3.81845C2.38483 2.79982 2.73793 1.78841 3.45107 1.05407C4.16421 0.319722 5.16485 -0.0628415 6.186 0.00844868C6.9284 0.00408689 7.67039 0.0441585 8.408 0.128449V2.69845L6.883 2.69845C6.4898 2.61523 6.08104 2.73438 5.79414 3.01585C5.50724 3.29732 5.3803 3.70373 5.456 4.09845L5.456 5.93845H8.308L7.936 8.81845H5.46L5.46 16.2084H2.486Z" fill="currentColor"></path>
                                              </svg>
                                              <span class="d-none">{{ 'general.social.links.facebook' | t }}</span>
                                            </a>
                                          </li>
                                        {%- endif -%}
                                        {%- if block.settings.social_instagram != blank -%}
                                          <li class="social-media__item social-media__item--instagram">
                                            <a href="{{ block.settings.social_instagram }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Instagram">
                                              <svg focusable="false" width="16" height="16" class="icon icon--instagram   " viewBox="0 0 16 16">
                                                <path fill-rule="evenodd" clip-rule="evenodd" d="M8 0C5.827 0 5.555.01 4.702.048 3.85.087 3.269.222 2.76.42a3.921 3.921 0 00-1.417.923c-.445.444-.719.89-.923 1.417-.198.509-.333 1.09-.372 1.942C.01 5.555 0 5.827 0 8s.01 2.445.048 3.298c.039.852.174 1.433.372 1.942.204.526.478.973.923 1.417.444.445.89.719 1.417.923.509.198 1.09.333 1.942.372C5.555 15.99 5.827 16 8 16s2.445-.01 3.298-.048c.852-.039 1.433-.174 1.942-.372a3.922 3.922 0 001.417-.923c.445-.444.719-.89.923-1.417.198-.509.333-1.09.372-1.942C15.99 10.445 16 10.173 16 8s-.01-2.445-.048-3.298c-.039-.852-.174-1.433-.372-1.942a3.922 3.922 0 00-.923-1.417A3.921 3.921 0 0013.24.42c-.509-.198-1.09-.333-1.942-.372C10.445.01 10.173 0 8 0zm0 1.441c2.136 0 2.39.009 3.233.047.78.036 1.203.166 1.485.276.374.145.64.318.92.598.28.28.453.546.598.92.11.282.24.705.276 1.485.038.844.047 1.097.047 3.233s-.009 2.39-.047 3.233c-.036.78-.166 1.203-.276 1.485-.145.374-.318.64-.598.92-.28.28-.546.453-.92.598-.282.11-.705.24-1.485.276-.844.038-1.097.047-3.233.047s-2.39-.009-3.233-.047c-.78-.036-1.203-.166-1.485-.276a2.479 2.479 0 01-.92-.598 2.478 2.478 0 01-.598-.92c-.11-.282-.24-.705-.276-1.485-.038-.844-.047-1.097-.047-3.233s.009-2.39.047-3.233c.036-.78.166-1.203.276-1.485.145-.374.318-.64.598-.92.28-.28.546-.453.92-.598.282-.11.705-.24 1.485-.276.844-.038 1.097-.047 3.233-.047zm0 9.226a2.667 2.667 0 110-5.334 2.667 2.667 0 010 5.334zm0-6.775a4.108 4.108 0 100 8.216 4.108 4.108 0 000-8.216zm5.23-.162a.96.96 0 11-1.92 0 .96.96 0 011.92 0z" fill="currentColor"></path>
                                              </svg>
                                              <span class="d-none">{{ 'general.social.links.instagram' | t }}</span>
                                            </a>
                                          </li>
                                        {%- endif -%}
                                        {%- if block.settings.social_pinterest != blank -%}
                                          <li class="social-media__item social-media__item--pinterest">
                                            <a href="{{ block.settings.social_pinterest }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Pinterest">
                                              <svg focusable="false" width="12" height="16" class="icon icon--pinterest   " viewBox="0 0 12 16">
                                                <path fill-rule="evenodd" clip-rule="evenodd" d="M5.8042 0.00123531C8.79537 -0.0442356 10.6685 1.16769 11.5498 3.29299C11.8407 3.99433 12.1516 5.28439 11.9181 6.35474C11.825 6.78208 11.7985 7.22812 11.6726 7.63086C11.4163 8.4496 11.0829 9.17441 10.6413 9.79945C10.0418 10.6486 9.13196 11.2212 7.98951 11.5091C6.97899 11.7637 6.04959 11.3826 5.50954 10.9732C5.33747 10.843 5.10674 10.6728 5.04304 10.4377C5.03488 10.4377 5.0267 10.4377 5.01853 10.4377C4.97972 10.8669 4.81532 11.3224 4.69924 11.7135C4.53858 12.2545 4.50733 12.8146 4.3064 13.3208C4.08349 13.8828 3.81274 14.3978 3.52072 14.8776C3.36739 15.1292 2.94427 15.9904 2.63675 16C2.60311 15.9354 2.58964 15.9105 2.58761 15.796C2.48858 15.6383 2.55757 15.3724 2.51393 15.1578C2.44604 14.8236 2.39317 14.2217 2.46491 13.8824C2.46491 13.7038 2.46491 13.5248 2.46491 13.3465C2.54397 12.9786 2.54085 12.6015 2.63675 12.2494C2.84537 11.4824 2.96145 10.6699 3.17692 9.87611C3.38398 9.11352 3.57396 8.27939 3.74172 7.50321C3.77957 7.32789 3.56652 6.82389 3.52072 6.63572C3.37628 6.04186 3.48624 5.21874 3.66805 4.77269C3.89698 4.21111 4.56717 3.3535 5.43589 3.57359C6.13407 3.75039 6.57846 4.50528 6.34437 5.46192C6.09862 6.46589 5.7798 7.3653 5.5587 8.37035C5.50173 8.62933 5.59968 8.90442 5.65687 9.05958C5.86357 9.61934 6.49037 10.163 7.32652 9.95278C8.59396 9.63365 9.15431 8.48627 9.53645 7.24791C9.63981 6.91302 9.62743 6.59647 9.70831 6.22709C9.87894 5.44763 9.80648 4.28411 9.56098 3.67556C9.16753 2.70023 8.43329 2.07518 7.42471 1.73624C7.1465 1.68526 6.86819 1.63427 6.58988 1.58329C6.12397 1.47655 5.23532 1.63685 4.92023 1.73624C3.51171 2.18156 2.63952 2.92544 2.09658 4.26247C1.91177 4.71767 1.81046 5.17911 1.77741 5.81884C1.76913 5.8955 1.76094 5.97217 1.75278 6.04883C1.86153 6.62068 1.87259 6.99959 2.09658 7.42657C2.20715 7.63711 2.46971 7.8029 2.51393 8.06444C2.54001 8.2185 2.42705 8.45105 2.39125 8.57467C2.33705 8.76137 2.35676 8.97522 2.26844 9.13625C2.10873 9.42678 1.67383 9.208