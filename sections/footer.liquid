{{ 'footer.css' | asset_url | stylesheet_tag }}
{% render 'section-style' %}
{%- assign id = '#shopify-section-' | append: section.id -%}

<style>

{{ id }} {
      {%- if section.settings.background_repeat -%}
        background-repeat: repeat;
      {%- else -%}
        background-repeat: no-repeat;
      {%- endif -%}
      {%- if section.settings.background_size != "default" and section.settings.background_size != "custom" -%}
        background-size: {{ section.settings.background_size }};
      {%- endif -%}
      {%- if section.settings.background_size == "custom" -%}
        background-size: {{ section.settings.background_size_custom }}%;
      {%- endif -%}
      {%- if section.settings.background_position != "custom" -%}
        background-position: {{ section.settings.background_position }};
      {%- else -%}
        background-position-x: {{ section.settings.background_position_x }};
        background-position-y: {{ section.settings.background_position_y }};
      {%- endif -%}
    }


    {%- for block in section.blocks -%}
      #block-{{ block.id }} .footer__blocks {
        {%- if block.settings.block_content_max_width > 0 -%}
          max-width: {{ block.settings.block_content_max_width | append: 'px' }};
        {%- endif -%}
        {%- if block.settings.block_padding_desktop != blank -%}
          padding: {{ block.settings.block_padding_desktop }};
        {%- endif -%}
        
        
        {%- if block.settings.background_image != blank -%}
          background-image: url({{ block.settings.background_image | image_url: width: 2000 }});
        {%- endif -%}
        border-radius: {{ block.settings.block_rounded |  append: 'px' }};
      }

      #block-{{ block.id }} .footer__logo {
        margin-bottom: {{ block.settings.logo_space | append: 'px' }};
      }
      #block-{{ block.id }} .footer__text {
        font-size: {{ block.settings.text_font_size | append: 'px' }};
        line-height: {{ block.settings.text_line_height | append: 'px' }};
        font-weight: {{ block.settings.text_font_weight }};
        color: {{ block.settings.color_text }};
        margin-bottom: {{ block.settings.text_space | append: 'px' }};
      }
      #block-{{ block.id }} .footer__heading {
        font-size: {{ block.settings.heading_font_size | append: 'px' }};
        line-height: {{ block.settings.heading_line_height | append: 'px' }};
        font-weight: {{ block.settings.heading_font_weight }};
        color: {{ block.settings.heading_color }};
        margin-bottom: {{ block.settings.heading_space | append: 'px' }};
      }
      {%- if block.settings.address_color != blank -%}
        #block-{{ block.id }} .footer__item-address {
          color: {{ block.settings.address_color }};
        }
      {%- endif -%}
      {%- if block.settings.phone_color != blank -%}
        #block-{{ block.id }} .footer__item-phone {
          color: {{ block.settings.phone_color }};
        }
        #block-{{ block.id }} .footer__item-phone:hover {
          color: {{ block.settings.phone_color }};
        }
      {%- endif -%}
      {%- if block.settings.email_color != blank -%}
        #block-{{ block.id }} .footer__item-email {
          color: {{ block.settings.email_color }};
        }
        #block-{{ block.id }} .footer__item-email:hover {
          color: {{ block.settings.email_color }};
        }
      {%- endif -%}
      #block-{{ block.id }} .footer__item-link .footer__item-icon svg {
        color: currentColor;
      }
      #block-{{ block.id }} .footer__title {
        font-size: {{ block.settings.title_font_size | append: 'px' }};
        line-height: {{ block.settings.title_line_height | append: 'px' }};
        font-weight: {{ block.settings.title_font_weight }};
        color: {{ block.settings.title_color }};
        margin-bottom: {{ block.settings.title_space | append: 'px' }};
      }
      #block-{{ block.id }} .linklist__item a {
        font-size: {{ block.settings.menu_font_size | append: 'px' }};
        line-height: {{ block.settings.menu_line_height | append: 'px' }};
        color: {{ block.settings.menu_item_color }};
        font-weight: {{ block.settings.menu_font_weight }};
      }
      #block-{{ block.id }} .linklist__item a:hover {
        color: {{ block.settings.menu_item_color_hover }};
      }

      {%- if block.type == 'footer_contact' -%}
        #block-{{ block.id }} .footer__item-link {
          font-size: {{ block.settings.phone_font_size | append: 'px' }};
          line-height: {{ block.settings.phone_line_height }};
          gap: {{ block.settings.icon_spacing | append: 'px' }};
        }

        #block-{{ block.id }} .footer__item-phone {
          margin-bottom: {{ block.settings.phone_spacing | append: 'px' }};
        }

        #block-{{ block.id }} .footer__item-email {
          font-size: {{ block.settings.email_font_size | append: 'px' }};
          line-height: {{ block.settings.email_line_height }};
          margin-bottom: 0;
        }

        #block-{{ block.id }} .footer__item-link span:not(.footer__item-icon) {
          display: inline;
          margin: 0;
          font-size: inherit;
          font-weight: 500;
          line-height: inherit;
        }

        #block-{{ block.id }} .footer__item-link .footer__item-icon {
          display: inline-flex;
          align-items: center;
          justify-content: center;
          margin: 0;
          line-height: 1;
        }

        #block-{{ block.id }} .footer__item-address {
          font-size: {{ block.settings.address_font_size | append: 'px' }};
          line-height: {{ block.settings.address_line_height }};
          margin-top: {{ block.settings.address_spacing | append: 'px' }};
        }

        #block-{{ block.id }} .footer__item-address strong {
          display: block;
          margin: {{ block.settings.address_spacing | append: 'px' }} 0 4px;
        }

        #block-{{ block.id }} .footer__item-address strong:first-child {
          margin-top: 0;
        }
      {%- endif -%}

      @media (max-width: 1199px) and (min-width: 992px) {
        
      }

      @media (max-width: 1199px) {
        {%- if block.settings.block_padding_tablet != blank -%}
          #block-{{ block.id }} .footer__blocks {
            padding: {{ block.settings.block_padding_tablet }};          
          }       
        {%- endif -%}
        {%- if block.settings.heading_font_size_tablet != blank -%}

          {%- liquid
            assign space = block.settings.heading_font_size | minus: block.settings.heading_font_size_tablet
            assign line_height_tablet = block.settings.heading_line_height | minus: space
          -%}

          #block-{{ block.id }} .footer__heading {
            font-size: {{ block.settings.heading_font_size_tablet | append: 'px' }};
            line-height: {{ line_height_tablet | append: 'px' }};
          }
        {%- endif -%}
      }

      @media (max-width: 767px) {
        {%- if block.settings.block_padding_mobile != blank -%}
          #block-{{ block.id }} .footer__blocks {
            padding: {{ block.settings.block_padding_mobile }};          
          }       
        {%- endif -%}
        {%- if block.settings.heading_font_size_mobile != blank -%}

          {%- liquid
            assign space = block.settings.heading_font_size | minus: block.settings.heading_font_size_mobile
            assign line_height_mobile = block.settings.heading_line_height | minus: space
          -%}

          #block-{{ block.id }} .footer__heading {
            font-size: {{ block.settings.heading_font_size_mobile | append: 'px' }};
            line-height: {{ line_height_mobile | append: 'px' }};
          }
        {%- endif -%}
      }

    {%- endfor -%}

</style>

<footer class="footer ">
  <div class="shopify-container">
    <div class="footer__inner">

        <div class="row">
          {%- for block in section.blocks -%}
            
            <div id="block-{{ block.id }}" class="col" >
  
              {%- case block.type -%}

                {%- when 'footer-logo' -%}
                  <div class="footer__blocks">
                    {%- if block.settings.logo != blank -%}
                      <div class="footer__logo">
                        {{ block.settings.logo |  image_url: width: block.settings.logo.width |  image_tag }}
                      </div>
                    {%- endif -%}
                    {%- if block.settings.footer_text != blank -%}
                      <div class="footer__text">
                        {{ block.settings.footer_text }}
                      </div>
                    {%- endif -%}
                    {%- if block.settings.button_content != blank -%}
                      <a href="{{ block.settings.button_url |  default: "#" }}" class="{{ block.settings.button_style }}" style="--btn-color: {{ block.settings.button_color }}; --btn-bg: {{ block.settings.button_bg_color }}; --btn-color-hover: {{ block.settings.button_color_hover }}; --btn-bg-hover: {{ block.settings.button_bg_color_hover }};">
                      <span class="icon-button">
                                  {%- render 'icon-arrow' -%}
                                </span>
                                {{ block.settings.button_content }}
                    </a>
                    {%- endif -%}

                    {% liquid
                      assign _show_social = false
                      if block.settings.social_facebook != blank
                        assign _show_social = true
                      endif
                      if section.settings.social_tw != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_instagram != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_pinterest != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_youtube != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_linkedin != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_tiktok != blank
                        assign _show_social = true
                      endif
                    %}


                     {%- if _show_social == true -%}
                      {%- if block.settings.social_heading != blank -%}
                        <div class="footer__item-title heading {{ block.settings.heading_size }}">{{ block.settings.social_heading }}</div>
                      {%- endif -%}
                      <div class="footer__item-content">
                        <ul class="social-media social-media--no-radius list--unstyled" role="list">
                          {%- if block.settings.social_facebook != blank -%}
                            <li class="social-media__item social-media__item--facebook">
                              <a href="{{ block.settings.social_facebook }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Facebook">
                                {% render 'icon-facebook' %}
                              <span class="text-social">{{ 'general.social.links.facebook' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_tw != blank -%}
                            <li class="social-media__item social-media__item--tw">
                              <a href="{{ block.settings.social_tw }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on tw">
                                {% render 'icon-twitter' %}
                                <span class="text-social">{{ 'general.social.links.twitter' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_instagram != blank -%}
                            <li class="social-media__item social-media__item--instagram">
                              <a href="{{ block.settings.social_instagram }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Instagram">
                                {% render 'icon-instagram' %}
                                <span class="text-social">{{ 'general.social.links.instagram' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_pinterest != blank -%}
                            <li class="social-media__item social-media__item--pinterest">
                              <a href="{{ block.settings.social_pinterest }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Pinterest">
                                {% render 'icon-pinterest' %}
                                <span class="text-social">{{ 'general.social.links.pinterest' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_youtube != blank -%}
                            <li class="social-media__item social-media__item--youtube">
                              <a href="{{ block.settings.social_youtube }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on YouTube">
                                {% render 'icon-youtube' %}
                                <span class="text-social">{{ 'general.social.links.youtube' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_linkedin != blank -%}
                            <li class="social-media__item social-media__item--linkedin">
                              <a href="{{ block.settings.social_linkedin }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on LinkedIn">
                                {% render 'icon-linkedin' %}
                                <span class="text-social">{{ 'general.social.links.linkedin' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_tiktok != blank -%}
                            <li class="social-media__item social-media__item--tiktok">
                              <a href="{{ block.settings.social_tiktok }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on TikTok">
                                {% render 'icon-tiktok' %}
                                <span class="text-social">TikTok</span>
                              </a>
                            </li>
                          {%- endif -%}
                        </ul>
                      </div>
                    {%- endif -%}
                  </div>
  
                {%- when 'footer_text' -%}
                  <div class="footer__blocks text-{{ block.settings.text_align_mobile }} text-md-{{ block.settings.text_align }}">
  
                    {%- if block.settings.footer_heading != blank -%}
                      <h6 class="footer__heading">{{ block.settings.footer_heading }}</h6>
                    {% endif %}
  
                    {%- if block.settings.footer_text != blank -%}
                      <div class="footer__text">{{ block.settings.footer_text }}</div>
                    {% endif %}

                    {%- if block.settings.button_content != blank -%}
                      <a href="{{ block.settings.button_url |  default: "#" }}" class="{{ block.settings.button_style }}" style="--btn-color: {{ block.settings.button_color }}; --btn-bg: {{ block.settings.button_bg_color }}; --btn-color-hover: {{ block.settings.button_color_hover }}; --btn-bg-hover: {{ block.settings.button_bg_color_hover }};">
                      <span class="icon-button">
                                  {%- render 'icon-arrow' -%}
                                </span>
                                {{ block.settings.button_content }}
                    </a>
                    {%- endif -%}      

                    {% liquid
                      assign _show_social = false
                      if block.settings.social_facebook != blank
                        assign _show_social = true
                      endif
                      if section.settings.social_tw != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_instagram != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_pinterest != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_youtube != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_linkedin != blank
                        assign _show_social = true
                      endif
                    %}
                    {%- if _show_social == true -%}
                      {%- if block.settings.social_heading != blank -%}
                        <div class="footer__item-title heading {{ block.settings.heading_size }}">{{ block.settings.social_heading }}</div>
                      {%- endif -%}
                      <div class="footer__item-content">
                        <ul class="social-media social-media--no-radius list--unstyled" role="list">
                          {%- if block.settings.social_facebook != blank -%}
                            <li class="social-media__item social-media__item--facebook">
                              <a href="{{ block.settings.social_facebook }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Facebook">
                                {% render 'icon-facebook' %}
                              <span class="text-social">{{ 'general.social.links.facebook' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_tw != blank -%}
                            <li class="social-media__item social-media__item--tw">
                              <a href="{{ block.settings.social_tw }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on tw">
                                {% render 'icon-twitter' %}
                                <span class="text-social">{{ 'general.social.links.twitter' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_instagram != blank -%}
                            <li class="social-media__item social-media__item--instagram">
                              <a href="{{ block.settings.social_instagram }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Instagram">
                                {% render 'icon-instagram' %}
                                <span class="text-social">{{ 'general.social.links.instagram' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_pinterest != blank -%}
                            <li class="social-media__item social-media__item--pinterest">
                              <a href="{{ block.settings.social_pinterest }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Pinterest">
                                {% render 'icon-pinterest' %}
                                <span class="text-social">{{ 'general.social.links.pinterest' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_youtube != blank -%}
                            <li class="social-media__item social-media__item--youtube">
                              <a href="{{ block.settings.social_youtube }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on YouTube">
                                {% render 'icon-youtube' %}
                                <span class="text-social">{{ 'general.social.links.youtube' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_linkedin != blank -%}
                            <li class="social-media__item social-media__item--linkedin">
                              <a href="{{ block.settings.social_linkedin }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on LinkedIn">
                                {% render 'icon-linkedin' %}
                                <span class="text-social">{{ 'general.social.links.linkedin' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                        </ul>
                      </div>
                    {%- endif -%}
  
                  </div>

                {%- when 'footer_text2' -%}
                  <div class="footer__item text-{{ block.settings.text_align }}">
                    {%- if block.settings.description != blank -%}
                      <div class="description" style="color: {{ block.settings.description_color }}">
                        {{ block.settings.description }}
                      </div>
                    {%- endif -%}

                    {%- if block.settings.btn_text != blank -%}
                      <div class="footer__item-button">
                        <a class="btn btn-primary" href="{{ block.settings.btn_url }}">
                          {{ block.settings.btn_text }}
                          {%- if block.settings.btn_icon != blank -%}
                            <span class="icon-button">
                              {{ block.settings.btn_icon }}
                            </span>
                          {%- endif -%}
                          
                        </a>
                      </div>
                    {%- endif -%}


                  </div>
  
                {%- when 'line' -%}
                  <style>
                    .border-{{ block.id }} .border-footer {
                      position: relative;
                      opacity: {{ block.settings.overlay_opacity | divided_by: 10.0 }};

                      {%- if block.settings.margin_desktop != blank -%}
                        margin: {{ block.settings.margin_desktop }};
                      {%- endif -%}

                      {%- if block.settings.padding_desktop != blank -%}
                        padding: {{ block.settings.padding_desktop }};
                      {%- endif -%}
                    }
                    .border-{{ block.id }} .border-footer::before {
                      content: '';
                      width: 100vw;
                      height: 1px;
                      background: {{ block.settings.bgborder }};
                      position: absolute;
                      top: 0;
                      left: 50%;
                      transform: translateX(-50%);
                    }
                    {% if block.settings.margin_mobile != blank or block.settings.padding_mobile != blank %}
                      @media(max-width: 767px){
                        .border-{{ block.id }} .border-footer {
                          {% if block.settings.margin_mobile != blank %}
                            margin: {{ block.settings.margin_mobile }};
                          {% endif %}
                          {% if block.settings.padding_mobile != blank %}
                            padding: {{ block.settings.padding_mobile }};
                          {% endif %}
                        }
                      }
                    {% endif %}
                  </style>
                  <div class="border-{{ block.id }}">
                    <div class="border-footer"></div>
                  </div>

                {%- when 'footer_contact' -%}
                  <div class="footer__item" {% if section.settings.data_aos != "none" %} data-aos="{{ section.settings.data_aos }}" {% endif %}>
                    {% if block.settings.heading != blank %}
                      <h6 class="footer__item-title">{{ block.settings.heading }}</h6>
                    {% endif %}
                    <div class="footer__item-content">
                      {% if block.settings.phone != blank %}
                        {%- assign phone_clean = block.settings.phone | remove: ' ' | remove: '-' | remove: '.' | remove: '(' | remove: ')' -%}
                        <a href="tel:{{ phone_clean }}" class="footer__item-phone footer__item-link">
                          <span class="footer__item-icon">{{ block.settings.phone_icon }}</span>
                          <span>{{ block.settings.phone }}</span>
                        </a>
                      {% endif %}
                      {% if block.settings.email != blank %}
                        {%- assign email_text = block.settings.email | remove: 'Email: ' | remove: 'Email:' | strip -%}
                        <a href="mailto:{{ email_text }}" class="footer__item-email footer__item-link">
                          <span class="footer__item-icon">{{ block.settings.email_icon }}</span>
                          <span>{{ block.settings.email }}</span>
                        </a>
                      {% endif %}
                      {% if block.settings.address != blank %}
                        <p class="footer__item-address">
                          {{ block.settings.address }}
                        </p>
                      {% endif %}
                    </div>
                  </div>
    
                {%- when 'footer_item' -%}
                  
                  <div class="footer__blocks text-{{ block.settings.text_align_mobile }} text-md-{{ block.settings.text_align }}">
  
                    {%- if block.settings.footer_heading != blank -%}
                      <h6 class="footer__heading ">{{ block.settings.footer_heading }}</h6>
                    {% endif %}
                    
                    <div class="footer__item-menu">
                      {%- if block.settings.menu != blank -%}
                        <ul class="linklist list--unstyled {{ block.settings.menu_direction }} {% if block.settings.menu_direction == 'horizontal' %} justify-content-{{ block.settings.text_align_mobile }} justify-content-md-{{ block.settings.text_align }} {% endif %}" style="--menu-space: {{ block.settings.menu_space |  append: 'px' }}" role="list">
                        {%- for link in block.settings.menu.links -%}
                          <li class="linklist__item">
                            <a href="{{ link.url }}" class="link--faded">{{ link.title | escape }}</a>
                          </li>    
                        {% endfor %}
                        </ul>
                      {% endif %}
                    </div>
                    
                  </div>
    
                {%- when 'newsletter' -%}
                  
                  <div class="footer__blocks newletter__block {% if block.settings.background_image != blank %}background{% endif %}">
                    <div class="footer__blocks--right">
                    {%- if block.settings.footer_title != blank -%}
                      <div class="footer__title">{{ block.settings.footer_title }}</div>
                    {%- endif -%}
                    {%- if block.settings.footer_heading != blank -%}
                      <h6 class="footer__heading">{{ block.settings.footer_heading }}</h6>
                    {%- endif -%}
                    </div>
                    <div class="footer__blocks--left">
                      {%- if block.settings.footer_text != blank -%}
                        <div class="footer__text">{{ block.settings.footer_text }}</div>
                      {%- endif -%}
                    <div {{ block.shopify_attributes }}>
                      {% form 'customer', class: 'newsletter-form' %}
                        <input type="hidden" name="form_type" value="customer">
                        <input type="hidden" name="utf8" value="âœ“">
                        <input type="hidden" name="contact[tags]" value="newsletter">
                        <input type="hidden" name="contact[context]" value="newsletter-{{ section.id }}">
      
                        <div class="wp-newsletter-input" style="background-color: {{ section.settings.bg_color }};">
                          <div class="input-row">
                            <div class="input input-mail">
                              <input style="background-color: {{ section.settings.bg_input }};" type="email" id="newsletter[{{ section.id }}][contact][email]" name="contact[email]" class="input__field" required="" placeholder="{{ 'newsletter.label' | t }}">
                              {% comment %} <label style="background-color: {{ section.settings.bg_input }};" for="newsletter[{{ section.id }}][contact][email]" class="input__label">{{ 'newsletter.label' | t }}</label> {% endcomment %}
                            </div>
                            <div class="input-icon d-none">
                              <svg xmlns="http://www.w3.org/2000/svg" fill="#000000" width="800px" height="800px" viewBox="0 0 32 32">
                                <title/>
                                <g data-name="Layer 2" id="Layer_2">
                                <path d="M29,11H3a1,1,0,0,0-1,1V29a1,1,0,0,0,1,1H29a1,1,0,0,0,1-1V12A1,1,0,0,0,29,11Zm-2.79,2L16,20.23,5.79,13ZM4,28V14.18l11.42,8.09a1,1,0,0,0,1.16,0L28,14.18V28Z"/>
                                </g>
                              </svg>
                            </div>
