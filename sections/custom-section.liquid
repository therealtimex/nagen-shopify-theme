{% render 'section-style' %}
{{ 'custom-section.css' | asset_url | stylesheet_tag }}
{%- assign id = '#shopify-section-' | append: section.id -%}

{% style %}
    {% assign section_min_height = section.settings.min_height %}
    

    {{ id }} {
      {%- if section.settings.background_repeat -%}
        background-repeat: repeat;
      {%- else -%}
        background-repeat: no-repeat;
      {%- endif -%}
      {%- if section.settings.background_size != "default" and section.settings.background_size != "custom" -%}
        background-size: {{ section.settings.background_size }};
      {%- endif -%}
      {%- if section.settings.background_size == "custom" -%}
        background-size: {{ section.settings.background_size_custom }}%;
      {%- endif -%}
      {%- if section.settings.background_position != "custom" -%}
        background-position: {{ section.settings.background_position }};
      {%- else -%}
        background-position-x: {{ section.settings.background_position_x }};
        background-position-y: {{ section.settings.background_position_y }};
      {%- endif -%}
    }
    {{ id }} .box__banner--2.custom__section {
      min-height: {{ section_min_height }}px;
      display: flex;
      align-items: center;


      background-image: url({{ section.settings.background_content_image | image_url: width: 2000 }});
      background-repeat: no-repeat;
      background-size: cover;
      background-position: center center;
      justify-content: center;
      padding: 0 20px;
      border-radius: 5px;
      overflow: hidden;
    }

    {{ id }} .accordion {
      --bs-accordion-btn-active-icon: url('data:image/svg+xml,<svg width="14" height="2" viewBox="0 0 14 2" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M13.5 1.75H12.75H1.25H0.5V0.25H1.25H12.75H13.5V1.75Z" fill="%23162339"/></svg>');
      --bs-accordion-btn-icon: url('data:image/svg+xml,<svg width="14" height="14" viewBox="0 0 14 14" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M7.75 1.25V6.25H12.75H13.5V7.75H12.75H7.75V12.75V13.5H6.25V12.75V7.75H1.25H0.5V6.25H1.25H6.25V1.25V0.5H7.75V1.25Z" fill="%23162339"/></svg>');
      --bs-accordion-btn-icon-transform: none;
      --bs-accordion-btn-icon-width: 14px;
    }


    {% for block in section.blocks %}
      {% liquid

          assign content_max_width = block.settings.content_max_width
  %}

  #block-{{ block.id }} {
    position: relative;
        {% if block.settings.z_index != 0 and block.settings.z_index != blank %}z-index: {{ block.settings.z_index }}; {% endif %}
  }
      
      #block-{{ block.id }} .block-content {
        {%- if block.settings.content_width != 0 -%}
          max-width: {{ block.settings.content_width | append: 'px' }};
        {%- endif -%}
        {%- if block.settings.block_padding_desktop != blank -%}
          padding: {{ block.settings.block_padding_desktop }};
        {%- endif -%}
        {%- if block.settings.content_height > 0 -%}
          height: {{ block.settings.content_height | append: '%' }};
        {%- endif -%}
        {%- if block.settings.block_rounded != blank -%}
          border-radius: {{ block.settings.block_rounded | append: 'px' }};
        {%- endif -%}
        {%- if block.settings.background_image != blank -%}
          background-image: url({{ block.settings.background_image |  image_url: width: 2000 }});
          background-repeat: no-repeat;
          background-size: cover;
          background-position: center center;
        {%- endif -%}
        
      }

      #block-{{ block.id }} .block-content.text-center {
        margin-left: auto;
        margin-right: auto;
      }

      {%  unless block.settings.content_full_width %}
          #block-{{ block.id }} .custom__text--content {
            max-width: {{ content_max_width }}px;
            margin: 0 auto;
          }
        {% endunless %}


      #block-{{ block.id }} .title_block {
        font-size: {{ block.settings.title_font_size |  append: 'px' }};
        line-height: {{ block.settings.title_line_height |  append: 'px' }};
        font-weight: {{ block.settings.title_font_weight }};
        text-transform: {{ block.settings.title_text_transform }};
        color: {{ block.settings.title_color }};
        margin-bottom: {{ block.settings.title_space |  append: 'px' }};

        display: flex;
        align-items: center;
        gap: 8px;
      }

      #block-{{ block.id }} .heading_block {
        font-size: {{ block.settings.heading_font_size |  append: 'px' }};
        line-height: {{ block.settings.heading_line_height |  default: '1' }};
        font-weight: {{ block.settings.heading_font_weight }};
        text-transform: {{ block.settings.heading_text_transform }};
        color: {{ block.settings.heading_color }};
        margin-bottom: {{ block.settings.heading_space |  append: 'px' }};
      }

      #block-{{ block.id }} .description {
        font-size: {{ block.settings.description_font_size |  append: 'px' }};
        line-height: {{ block.settings.description_line_height |  append: 'px' }};
        color: {{ block.settings.description_color }};
        margin-bottom: {{ block.settings.description_space |  append: 'px' }};
      }
      #block-{{ block.id }} .description_2 {
        font-size: {{ block.settings.description_font_size_2 |  append: 'px' }};
        line-height: {{ block.settings.description_line_height_2 |  append: 'px' }};
        color: {{ block.settings.description_color_2 }};
        margin-bottom: {{ block.settings.description_space_2 |  append: 'px' }};
      }

      @media (max-width: 1199px) {
        #block-{{ block.id }} .heading_block {
          font-size: {{ block.settings.heading_font_size_tablet |  append: 'px' }};
          margin-bottom: {{ block.settings.heading_space_tablet |  append: 'px' }};
        }
      }
      @media (max-width: 767px) {
        #block-{{ block.id }} .heading_block {
          font-size: {{ block.settings.heading_font_size_mobile |  append: 'px' }};
          margin-bottom: {{ block.settings.heading_space_mobile |  append: 'px' }};
        }
      }


      #block-{{ block.id }} .custom__text--button .btn-primary {
        background: {{ block.settings.button_bg_color }};
        color: {{ block.settings.button_color }}
      }

      #block-{{ block.id }} .custom__text--button .btn-primary:hover {
        background: {{ block.settings.button_bg_color_hover }};
        color: {{ block.settings.button_color_hover }}
      }

      #block-{{ block.id }} .list__text {
        display: grid;
        gap: {{ block.settings.list_text_item_space |  append: 'px' }} 30px;
        margin-bottom: {{ block.settings.list_text_space | append: 'px' }};
        font-size: {{ block.settings.list_text_font_size |  append: 'px' }};
        line-height: {{ block.settings.list_text_line_height |  append: 'px' }};
        color: {{ block.settings.list_text_color }};
        @media (min-width: 480px) {
          grid-template-columns: repeat( {{ block.settings.list_text_column }}, minmax(0, 300px));
        }
      }
      #block-{{ block.id }} .list__text--item span {
        margin-right: {{ block.settings.icon_list_text_space |  append: 'px' }};
        color: {{ block.settings.icon_list_text_color }};
      }

      #block-{{ block.id }} .main__image img {
         {%- if block.settings.image_width > 0 -%}
          width: {{ block.settings.image_width |  append: '%' }};
        {%- endif -%};
        
      
        border-radius: {{ block.settings.image_rounded |  append: 'px' }};
        {%- if block.settings.image_full_height -%}
          height: 100%;
          object-fit: cover;
        {%- endif -%}
      }

      #block-{{ block.id }} .image__item_1 img {
        border-radius: {{ block.settings.image_rounded |  append: 'px' }};
      }

      @media (max-width: 1199px) {
        #block-{{ block.id }} .block-content {
          {%- if block.settings.block_padding_tablet != blank -%}
            padding: {{ block.settings.block_padding_tablet }};
          {%- endif -%}
      }
      }
      


      @media (max-width: 767px) {
        
        #block-{{ block.id }} .block-content {
          {%- if block.settings.block_padding_mobile != blank -%}
          padding: {{ block.settings.block_padding_mobile }};
        {%- endif -%}
        }
      }

      {%- if block -%}
        
      {%- endif -%}

        /* block icon box */

        #block-{{ block.id }} .icon_box {
          padding: {{ block.settings.padding_desktop }};
          border-radius: {{ block.settings.border_radius |  append: 'px' }};
          background: {{ block.settings.bg_color }};
          position: relative;
          transition: 0.3s all ease;
        }

        #block-{{ block.id }} .icon_box-icon {
          display: flex;
          justify-content: center;
          align-items: center;
          width: {{ block.settings.icon_size |  append: 'px' }};
          height: {{ block.settings.icon_size |  append: 'px' }};
          {% if block.settings.icon_style == 'circle' %}
            border-radius: 50%;
          {%  else %}
            border-radius: {{ block.settings.icon_border_radius }};
          {% endif %}
          background-color: {{ block.settings.icon_bg_color }};
          color: {{ block.settings.icon_color }};
          margin-bottom: {{ block.settings.icon_space |  append: 'px' }};
          {%- if block.settings.icon_x_position != 0 or block.settings.icon_y_position != 0 -%}
            position: absolute;
            top: {{ block.settings.icon_y_position |  append: 'px' }};
            left: {{ block.settings.icon_x_position |  append: 'px' }};
          {%- endif -%}
          transition: 0.3s all ease;
        }

        #block-{{ block.id }} .icon_box-title {
          font-size: {{ block.settings.title_font_size |  append: 'px' }};
          line-height: {{ block.settings.title_line_height |  append: 'px' }};
          font-weight: {{ block.settings.title_font_weight }};
          text-transform: {{ block.settings.title_text_transform }};
          color: {{ block.settings.title_color }};
          margin-bottom: {{ block.settings.title_space |  append: 'px' }};
          transition: 0.3s all ease;
        }

        #block-{{ block.id }} .icon_box-description {
          font-size: {{ block.settings.description_font_size |  append: 'px' }};
          line-height: {{ block.settings.description_line_height |  append: 'px' }};
          color: {{ block.settings.description_color }};
          margin-bottom: {{ block.settings.description_space |  append: 'px' }};
          transition: 0.3s all ease;
        }

        #block-{{ block.id }} .icon_box:hover {
          background: {{ block.settings.bg_color_hover }};
        }

        #block-{{ block.id }} .icon_box:hover a {
          color: {{ block.settings.color_hover }};
        }
        #block-{{ block.id }} .icon_box:hover .icon_box-title {
          color: {{ block.settings.color_hover }};
        }
        #block-{{ block.id }} .icon_box:hover .icon_box-description {
          color: {{ block.settings.color_hover }};
        }


        @media (max-width: 1199px) {
          #block-{{ block.id }} .icon_box {
            padding: {{ block.settings.padding_tablet }};
          }
        }

        @media (max-width: 767px) {
          #block-{{ block.id }} .icon_box {
            padding: {{ block.settings.padding_mobile }};
          }
        }


        /* end block icon box */

        #block-{{ block.id }} .icon__box {
         gap: {{ block.settings.icon_box_space |  append: 'px' }};
         margin-bottom: {{ block.settings.icon_box_space |  append: 'px' }};
        }

        #block-{{ block.id }} .icon__box--icon svg {
          width: {{ block.settings.icon_box_size |  append: 'px' }};
          color: {{ block.settings.icon_box_color }};
        }

        #block-{{ block.id }} .icon__box--title {
          font-size: {{ block.settings.icon_box_title_font_size |  append: 'px' }};
          line-height: {{ block.settings.icon_box_title_line_height |  append: 'px' }};
          font-weight: {{ block.settings.icon_box_title_font_weight }};
          color: {{ block.settings.icon_box_title_color }};
          margin-bottom: {{ block.settings.icon_box_title_space |  append: 'px' }};
        }

        #block-{{ block.id }} .icon__box--description {
          font-size: {{ block.settings.icon_box_description_font_size |  append: 'px' }};
          line-height: {{ block.settings.icon_box_description_line_height |  append: 'px' }};
          color: {{ block.settings.icon_box_description_color }};
        }

        #block-{{ block.id }} .testimonial_2 {
          background: none;
          position: relative;
          height: 100%;
        }

        #block-{{ block.id }} .testimonial_2:before {
          content: "";
          position: absolute;
          top: 0;
          right: 0;
          width: calc(100% + 50px);
          height: 100%;
          background-image: url({{ block.settings.background_image |  image_url: width: 2000 }});
          background-repeat: no-repeat;
          background-size: cover;
          background-position: right center;
          border-radius: 0 20px 20px 0;
        }

        #block-{{ block.id }} .image-box-title {
          font-size: {{ block.settings.service_label_size |  append: 'px' }};
          line-height: {{ block.settings.service_label_height |  append: 'px' }}
        }

        #block-{{ block.id }} .image-box-content {
          min-width: {{ block.settings.service_content_width |  append: 'px' }}
        }

        @media (max-width: 767px) {
          #block-{{ block.id }} .testimonial_2:before {
            width: 100%;
            border-radius: 20px;
          }
        }

    {% endfor %}
{% endstyle %}

<div class="shopify-container">

<div class="custom__section {{ section.settings.class }}">
  {%- case section.settings.section_header_type -%}
    {%- when 'section-header-2' -%}
      {%- render 'section-header-2' -%}
    {%- when 'section-header-3' -%}
      {%- render 'section-header-3' -%}
    {%- else -%}
      {%- render 'section-header' -%}
  {%- endcase -%}

  

  


  <div class="row {% if section.settings.content_positon != 'start' %} align-items-{{ section.settings.content_positon }} {% endif %} {% if section.settings.no_gutter %} no-gutters {% endif %}">
    {% for block in section.blocks %}
      <div
        id="block-{{ block.id }}"
        class="col"
        {% if block.settings.data_aos != 'none' %}
          data-aos="{{ block.settings.data_aos }}"
        {% endif %}
      >
      <div class="block-content text-{{ block.settings.text_position }} {% if block.settings.class != blank %} {{ block.settings.class }} {% endif %} {%- if block.settings.image_full_height -%}h-100
            {%- endif -%}" >
        {%- case block.type -%}
          {%- when 'text' -%}
            
            <div class="custom__text--content d-flex flex-column text-md-{{ block.settings.text_position }} text-{{ block.settings.text_mobile_position }}">
              {% if block.settings.title_content != blank %}
                <div class="title_block justify-content-md-{{ block.settings.text_position }} justify-content-{{ block.settings.text_mobile_position }}">
                  {{ block.settings.title_content }}
                </div>
              {% endif %}
              {% if block.settings.heading_content != blank %}
                <h3 class="heading_block">
                  {{ block.settings.heading_content }}
                </h3>
              {% endif %}
              {% if block.settings.description_content != blank %}
                <div class="text_description description">
                  {{ block.settings.description_content }}
                </div>
              {% endif %}


              {%- if block.settings.icon_box_icon != blank or block.settings.icon_box_title != blank -%}
                <div class="icon__box d-flex {% if block.settings.icon_box_vertical %} flex-column {% endif %} align-items-{{ block.settings.icon_box_align }}">
                  <span class="icon__box--icon">
                    {%- if block.settings.icon_image_box_icon != blank -%}
                        {{ block.settings.icon_image_box_icon |  image_url: width: block.settings.icon_image_box_icon.width |  image_tag }}
                      {%- else -%}
                        {{ block.settings.icon_box_icon }}
                    {%- endif -%}
                  </span>
                  <div class="icon__box--content">
                    <h6 class="icon__box--title">
                      {{ block.settings.icon_box_title }}
                    </h6>
                    <div class="icon__box--description">
                      {{ block.settings.icon_box_description }}
                    </div>
                  </div>
                </div>
              {%- endif -%}


              {% if block.settings.description_content_2 != blank %}
                <div class="description_2">
                  {{ block.settings.description_content_2 }}
                </div>
              {% endif %}



              {%- assign listText = block.settings.list_text_item_1 | append: '|' |  append: block.settings.list_text_item_2 | append: '|' |  append: block.settings.list_text_item_3 | append: '|' |  append: block.settings.list_text_item_4 | append: '|' |  append: block.settings.list_text_item_5 | append: '|' |  append: block.settings.list_text_item_6 | append: '|' |  append: block.settings.list_text_item_7 | append: '|' |  append: block.settings.list_text_item_8 | append: '|' |  append: block.settings.list_text_item_9 |  split: '|' -%}

              <div class="list__text">
              {%- for item in listText -%}
                {%- if item != blank -%}
                  <div class="list__text--item d-flex align-items-center">
                    <span class="d-flex">{{ block.settings.list_text_icon }}</span>
                    {{ item }}
                  </div>
                {%- endif -%}
              {%- endfor -%}
              </div>



              

              


              {% if block.settings.date_picker != blank and block.settings.time_picker != blank %}
                <div class="custom__countdown">
                  {%- if block.settings.title_countdown != blank -%}
                    <h3 class="custom__countdown--title">
                    {{ block.settings.title_countdown }}
                  </h3>
                  {%- endif -%}
                  
                  {% render 'collection-countdown'
                    , settings: block.settings %}
                </div>
              {% endif %}


              {%- if block.settings.testimonial_content != blank -%}
                <div class="custom__testtimonial">
                  {%- if block.settings.icon_quote != blank -%}
                    <div class="icon_quote">
                      {{ block.settings.icon_quote }}
                    </div>
                  {%- endif -%}
                  {%- if block.settings.show_rate -%}
                    <div class="rating">
                      {%- for i in (1..block.settings.rating) -%}
                        <svg width="12" height="11" viewBox="0 0 12 11" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M9.16966 6.81574L11.7859 4.72705C12.1884 4.39725 11.9871 3.8476 11.484 3.8476L8.16342 3.62773C7.96217 3.62773 7.76092 3.5178 7.76092 3.29794L6.45281 0.329794C6.25156 -0.109931 5.64782 -0.109931 5.44657 0.329794L4.3397 3.29794C4.23908 3.5178 4.03783 3.62773 3.83658 3.62773L0.515989 3.8476C0.0128689 3.8476 -0.188379 4.39725 0.214117 4.72705L2.83034 6.81574C2.93097 6.92567 3.03159 7.0356 2.93097 7.25547L2.12597 10.4435C2.02535 10.7733 2.52847 11.213 2.93097 10.8832L5.74844 9.1243C5.94969 9.01437 6.15094 9.01437 6.35219 9.1243L9.06903 10.8832C9.47153 11.1031 9.97465 10.7733 9.87403 10.3335L9.06903 7.25547C8.96841 7.14553 9.06903 6.92567 9.16966 6.81574Z" fill="#FA9A00"/>
                        </svg>

                      {%- endfor -%}

                      {%- assign index = block.settings.rating | plus : 1 -%}

                      {%- for i in (index..5) -%}
                        <svg width="12" height="11" viewBox="0 0 12 11" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path fill-rule="evenodd" clip-rule="evenodd" d="M8.48473 6.08296L10.1408 4.76085L8.12175 4.62716C7.8643 4.62014 7.56372 4.54716 7.29797 4.35361C7.05297 4.17517 6.85932 3.90512 6.78925 3.57281L5.985 1.74793L5.26404 3.68122L5.249 3.71409C4.98386 4.29343 4.43607 4.61207 3.87815 4.62717L1.85924 4.76085L3.51527 6.08296L3.56799 6.14055C3.56953 6.14224 3.57181 6.14465 3.57475 6.14776C3.61718 6.19261 3.79713 6.38283 3.89516 6.65056C4.01207 6.96987 3.98755 7.28736 3.88382 7.56648L3.42129 9.39823L5.24354 8.26063L5.26905 8.2467C5.76906 7.97357 6.33157 7.97357 6.83157 8.2467L6.86432 8.26458L8.59179 9.38296L8.13662 7.6425C7.96008 7.26825 8.041 6.89586 8.08741 6.74377C8.15947 6.5076 8.28936 6.2964 8.43202 6.14055L8.48473 6.08296ZM9.06903 7.25547L9.87403 10.3335C9.97465 10.7733 9.47153 11.1031 9.06903 10.8832L6.35219 9.1243C6.15094 9.01437 5.94969 9.01437 5.74844 9.1243L2.93097 10.8832C2.52847 11.213 2.02535 10.7733 2.12597 10.4435L2.93097 7.25547C3.03159 7.0356 2.93097 6.92567 2.83034 6.81574L0.214117 4.72705C-0.188379 4.39725 0.0128689 3.8476 0.515989 3.8476L3.83658 3.62773C4.03783 3.62773 4.23908 3.5178 4.3397 3.29794L5.44657 0.329794C5.64782 -0.109931 6.25156 -0.109931 6.45281 0.329794L7.76092 3.29794C7.76092 3.5178 7.96217 3.62773 8.16342 3.62773L11.484 3.8476C11.9871 3.8476 12.1884 4.39725 11.7859 4.72705L9.16966 6.81574C9.06903 6.92567 8.96841 7.14553 9.06903 7.25547Z" fill="#C4C4C4"/>
                      </svg>

                      {%- endfor -%}
                    </div>
                  {%- endif -%}
                  <div class="content">{{ block.settings.testimonial_content }}</div>
                  <div class="author">
                    <div class="avt">
                      {{ block.settings.author_image |  image_url: width: block.settings.author_image.width |  image_tag }}
                    </div>
                    <div class="author-info">
                      <div class="name">{{ block.settings.author_name }}</div>
                      <div class="job">{{ block.settings.author_job }}</div>
                    </div>
                  </div>
                </div>
              {%- endif -%}

              {%- if block.settings.service_label_1 != blank or block.settings.service_label_2 != blank -%}
                <div class="box_images">
                
                  <div class="image-box">
                    <div class="image-box-img">
                      {%- if block.settings.service_icon_1 != blank -%}
                        {{ block.settings.service_icon_1 |  image_url: width: block.settings.service_icon_1.width |  image_tag }}
                        {% else %}
                        {{ block.settings.service_icon_svg_1 }}
                      {%- endif -%}
                      
                    </div>
                    <div class="image-box-content">
                      {%- if block.settings.service_label_1 != blank -%}
                        <h6 class="image-box-title" style="color: {{ block.settings.service_label_color }}">{{ block.settings.service_label_1 }}</h6>
                      {%- endif -%}
                      {%- if block.settings.service_text_1 != blank -%}
                        <p class="image-box-description" style="color: {{ block.settings.service_text_color }}">
                          {{ block.settings.service_text_1 }}
                        </p>
                      {%- endif -%}
                    </div>
                  </div>
                
                
                  <div class="image-box">
                    <div class="image-box-img">
                      {%- if block.settings.service_icon_2 != blank -%}
                        {{ block.settings.service_icon_2 |  image_url: width: block.settings.service_icon_2.width |  image_tag }}
                        {% else %}
                        {{ block.settings.service_icon_svg_2 }}
                      {%- endif -%}
                      
                    </div>
                    <div class="image-box-content">
                      {%- if block.settings.service_label_2 != blank -%}
