                    <script>
                    const steps = document.querySelectorAll('.step-item');
                    const stepTimeline = document.querySelector('.step__timeline');

                    function activateStepOnScroll() {
                      let scrollPos = window.innerHeight / 2;


                      const rectTimeline = stepTimeline.getBoundingClientRect();

                      let timePercent = 0;

                      if(rectTimeline.top < scrollPos) {
                        timePercent = (scrollPos - rectTimeline.top) / rectTimeline.height * 100
                      }


                      if(timePercent > 100) {
                        timePercent = 100
                      }
                      
                      stepTimeline.style.setProperty("--time-percent", `${timePercent}%`);
                      
                      
                      

                      steps.forEach((step) => {
                        const rect = step.getBoundingClientRect();
                     

                        if (rect.top < scrollPos) {
                          step.classList.add('active');
                        } else {
                          step.classList.remove('active');

                        }
                      });
                    }

                    window.addEventListener('scroll', activateStepOnScroll);
