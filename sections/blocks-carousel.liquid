{% render 'section-style' %}
{{ 'blocks-carousel.css' | asset_url | stylesheet_tag }}

{%- assign id = '#shopify-section-' | append: section.id -%}


{% liquid

  assign section_header_space = section.settings.header_space
%}

{% style %}


{{ id }} {
      {%- if section.settings.background_repeat -%}
        background-repeat: repeat;
      {%- else -%}
        background-repeat: no-repeat;
      {%- endif -%}
      {%- if section.settings.background_size != "default" and section.settings.background_size != "custom" -%}
        background-size: {{ section.settings.background_size }};
      {%- endif -%}
      {%- if section.settings.background_size == "custom" -%}
        background-size: {{ section.settings.background_size_custom }}%;
      {%- endif -%}
      {%- if section.settings.background_position != "custom" -%}
        background-position: {{ section.settings.background_position }};
      {%- else -%}
        background-position-x: {{ section.settings.background_position_x }};
        background-position-y: {{ section.settings.background_position_y }};
      {%- endif -%}
    }

 

  {% for block in section.blocks %}

    {% liquid  
      assign image_space = block.settings.image_mb

      assign collection_title_size = block.settings.title_size
      assign collection_title_height = block.settings.title_height
      assign collection_title_color = block.settings.title_color
      assign collection_title_space = block.settings.title_mb | append: 'px'

      assign collection_sub_size = block.settings.sub_title_size
      assign collection_sub_height = block.settings.sub_title_height
      assign collection_sub_color = block.settings.sub_title_color

      assign content_padding = block.settings.content_padding
      assign rounded = block.settings.rounded | append: 'px'
      assign bg_color = block.settings.bg_color
%}

    #block-{{ block.id }}.collection__item {
      padding: {{ content_padding }};
      border-radius: {{ rounded }};
      background-color: {{ bg_color }};
      display: flex;
      align-items: center;
      height: 100%;
      gap: 10px;
    }

    #block-{{ block.id }}.carousel__item {
      {%- if block.settings.item_direction == 'row' or block.settings.item_direction_reverse -%}
        display: flex;
        
        {%- if block.settings.item_direction_reverse -%}
            flex-direction: {{ block.settings.item_direction }}-reverse;
          {%- else -%}
            flex-direction: {{ block.settings.item_direction }};
        {%- endif -%}
      {%- endif -%}
      border-radius: {{ block.settings.item_rounded |  append: 'px' }};
      background: {{ block.settings.item_background }};
    }


    #block-{{ block.id }}
    .collection__item--image {
      margin: 0 0{{ image_space }}px;
      max-width: 50%;
    }
    #block-{{ block.id }}
    .collection__item--content {
      flex: 1;
    }
    #block-{{ block.id }}
    .collection__name {
      font-size: {{ collection_title_size }}px;
      line-height: {{ collection_title_height }}%;
      color: {{ collection_title_color }};
      margin: 0 0{{ collection_title_space }};
      font-family: var(--hd-font-family);
      font-weight: 500;
    }
    #block-{{ block.id }}
    .collection__count {
      font-size: {{ collection_sub_size }}px;
      line-height: {{ collection_sub_height }}%;
      color: {{ collection_sub_color }};
    }

    #block-{{ block.id }} .carousel__item--content {
      {%- if block.settings.content_absolute -%}
        position: absolute;
        {%- else -%}
        position: relative;
        z-index: 2;
      {%- endif -%}
      padding: {{ block.settings.content_padding_desktop }};
      margin: {{ block.settings.content_margin_desktop }};
      border-radius: {{ block.settings.content_rounded |  append: 'px' }};
      background-color: {{ block.settings.content_background_color }};
      display: flex;
      gap: {{ block.settings.content_icon_space |  append: 'px' }};
      {%- unless block.settings.content_direction_horizontal -%}
      flex-direction: column;
      {%- endunless -%}


      /* display: none; */
    }

    {%- if block.settings.content_direction_horizontal or block.settings.content_full_height -%}
      #block-{{ block.id }} .carousel__item--wrapper {
        flex: 1;
      }
    {%- endif -%}

    #block-{{ block.id }} .carousel__item--image {
      border-radius: {{ block.settings.image_rounded |  append: 'px' }}
    }

    {%- if block.settings.image_full_height -%}
       #block-{{ block.id }} .carousel__item--image img {
        height: 100%;
       }
    {%- endif -%}

    #block-{{ block.id }} .carousel__item--title {
      margin-bottom: {{ block.settings.heading_space |  append: 'px' }};
      font-size: {{ block.settings.heading_font_size |  append: 'px' }};
      line-height: {{ block.settings.heading_line_height |  append: 'px' }};
      font-weight: {{ block.settings.heading_font_weight }};
      text-transform: {{ block.settings.heading_text_transform }};
    }

    #block-{{ block.id }} .carousel__item--description {
      font-size: {{ block.settings.text_font_size |  append: 'px' }};
      line-height: {{ block.settings.text_line_height |  append: 'px' }};
      font-weight: {{ block.settings.text_font_weight }};
      text-transform: {{ block.settings.text_text_transform }};
      margin-bottom: {{ block.settings.text_space |  append: 'px' }}
    }

    
    

    {%- if block.settings.enable_index -%}
      #block-{{ block.id }} .carousel__item--icon {
        display: flex;
        align-items: center;
        justify-content: space-between;
        font-size: 20px;
        line-height: 28px;
        font-weight: 500;
        color: #999999;
      }
    {%- endif -%}

    
      .carousel__item--image::after, .carousel__item--image::before {
        content: ""
      }


    {%- if block.settings.content_dropdown_hover != blank -%}
      #block-{{ block.id }}.carousel__item:hover .carousel__item--content {
        box-shadow: {{ block.settings.content_dropdown_hover }};
      }
    {%- endif -%}



    @media (max-width: 1199px) {
      #block-{{ block.id }} .carousel__item--content {
        padding: {{ block.settings.content_padding_tablet }};
        margin: {{ block.settings.content_margin_tablet }};
      }
    }
    @media (max-width: 767px) {
      #block-{{ block.id }} .carousel__item--content {
        padding: {{ block.settings.content_padding_mobile }};
        margin: {{ block.settings.content_margin_mobile }};
      }
    }


  {% endfor %}

 

    /* Navigation Arrows */
    {{ section_selector }} .swiper-arrow {
      position: absolute;
      top: 50%;
      transform: translateY(-50%);
      left: -15px;
      right: -15px;
      display: flex;
      justify-content: space-between;
      pointer-events: none;
      z-index: 10;
    }

    {{ section_selector }} .swiper-button-prev,
    {{ section_selector }} .swiper-button-next {
      width: 32px;
      height: 32px;
      border-radius: 50%;
      border: 1px solid #d7dde6;
      background: #fff;
      display: grid;
      place-items: center;
      cursor: pointer;
      transition: background 0.2s ease, color 0.2s ease, border-color 0.2s ease;
      pointer-events: auto;
      color: #333;
    }

    {{ section_selector }} .swiper-button-prev:hover,
    {{ section_selector }} .swiper-button-next:hover {
      background: var(--primary);
      color: #fff;
      border-color: var(--primary);
    }

    {{ section_selector }} .swiper-button-prev::after,
    {{ section_selector }} .swiper-button-next::after {
      display: none;
    }

    {{ section_selector }} .swiper-button-disabled {
      opacity: 0.3;
      cursor: not-allowed;
    }

    @media (max-width: 767px) {
      {{ id }} .swiper-arrow {
        display: flex;
        justify-content: space-between;
        align-items: center;
        position: absolute;
        top: 45%;
        left: -10px;
        right: -10px;
        width: auto;
        z-index: 20;
        pointer-events: none;
        transform: translateY(-50%);
      }

      {{ id }} .swiper-button-prev,
      {{ id }} .swiper-button-next {
        position: relative !important;
        left: auto !important;
        right: auto !important;
        transform: none !important;
        margin: 0;
        width: 32px;
        height: 32px;
        box-shadow: 0 2px 6px rgba(0,0,0,0.1);
        pointer-events: auto;
        visibility: visible;
        opacity: 1;
        background: #fff;
        color: #1d2a38;
        border-radius: 50%;
        border: 1px solid #d7dde6;
        display: flex;
        justify-content: center;
        align-items: center;
        transition: background 0.2s ease, color 0.2s ease, border-color 0.2s ease;
      }

      {{ id }} .swiper-button-prev:hover,
      {{ id }} .swiper-button-next:hover {
        background: #21395d;
        color: #fff;
        border-color: #21395d;
      }

      {{ id }} .swiper-button-prev.swiper-button-disabled,
      {{ id }} .swiper-button-next.swiper-button-disabled {
        opacity: 0.35;
        pointer-events: none;
        border-color: #d7dde6;
      }
    }

{% endstyle %}

<div class="blocks__carousel {% if section.settings.class != blank %}{{ section.settings.class }}{% endif %}">
  
    <div class="shopify-container">
  
  {%- case section.settings.section_header_type -%}
    {%- when 'section-header-2' -%}
      {%- render 'section-header-2' -%}
    {%- when 'section-header-3' -%}
      {%- render 'section-header-3' -%}
    {%- else -%}
      {%- render 'section-header' -%}
  {%- endcase -%}

  <div class="blocks__swiper position-relative {% if section.settings.swiper_effect == 'zigzag' %}swiper-zigzag{% endif %}">
    <div
      id="swiper-{{ section.id }}"
      class="ap-swiper swiper"
      data-desktop-item="{{ section.settings.desktop_item }}"
      data-laptop-item="{{ section.settings.laptop_item }}"
      data-tablet-item="{{ section.settings.tablet_item }}"
      data-tabletsmall-item="{{ section.settings.tablet_small_item }}"
      data-mobile-item="{{ section.settings.mobile_item }}"
      data-loop="{{ section.settings.loop }}"
      data-rows="{{ section.settings.rows }}"
      data-direction="{{ section.settings.direction }}"
      data-navigation="{{ section.settings.navigation }}"
      data-pagination="{{ section.settings.pagination }}"
      data-space="{{ section.settings.space }}"
      data-space-desktop="{{ section.settings.space_desktop }}"
      data-time="{{ section.settings.time }}"
      data-effect="{{ section.settings.swiper_effect }}"
      >
      <div class="swiper-wrapper align-items-{{ section.settings.swiper_align }}">
        {% for block in section.blocks  %}
          <div class="swiper-slide">
            {%- case block.type -%}
              {%- when 'collection-item' -%}
                <div
                  id="block-{{ block.id }}"
                  class="collection__item text-{{ block.settings.text_align }}"
                  {% if block.settings.data_aos !="none" %}
                  data-aos="{{ block.settings.data_aos }}"
                  {% endif %}>
                  <div class="collection__item--image">
                    <a href="{{ block.settings.collection.url }}">

                      {%- if block.settings.image -%}
                        {{ block.settings.image | image_url: width: block.settings.image.width | image_tag }}
                      {% else %}
                        {% if block.settings.collection.image != blank %}
                          {{ block.settings.collection.image | image_url: width: block.settings.collection.image.width | image_tag }}
                        {% else %}
                          {{ 'collection-1' | placeholder_svg_tag }}
                        {% endif %}
                      {%- endif -%}
                    </a>
                  </div>
                  <div class="collection__item--content">
                    <h5 class="collection__name">
                      <a href="{{ block.settings.collection.url }}">{{ block.settings.collection.title }}</a>
                    </h5>
                    {% if block.settings.show_sub %}
                      <p class="collection__count">{{ block.settings.collection.all_products_count }} {{ block.settings.text_count }}</p>
                    {% endif %}

                  </div>
                </div>
              {%- when 'carousel-item' -%}
                <div
                  id="block-{{ block.id }}"
                  class="carousel__item text-{{ block.settings.text_align }} {% if block.settings.item_hover_effect %}hover-effect{% endif %}"
                  {% if block.settings.data_aos !="none" %}
                  data-aos="{{ block.settings.data_aos }}"
                  {% endif %}>
                  <div class="carousel__item--image">
                    {% if block.settings.url != blank %}
                      <a href="{{ block.settings.url }}">
                    {% endif %}
                    {% if block.settings.image != blank %}
                      {{ block.settings.image | image_url: width: block.settings.image.width | image_tag: loading: 'eager' }}
                    {% else %}
                      {{ 'image' | placeholder_svg_tag }}
                    {% endif %}
                    {% if block.settings.url != blank %}
                      </a>
                    {% endif %}

                  </div>
                  <div class="carousel__item--content">
                    {%- if block.settings.icon != blank -%}
                      <div class="carousel__item--icon">
                        {{ block.settings.icon |  image_url: width: block.settings.icon.width |  image_tag: loading: 'eager' }}

                        {%- if block.settings.enable_index -%}
                          <span>{%- if forloop.index < 10 -%}
                            0
                          {%- endif -%}{{ forloop.index }}</span>
                        {%- endif -%}
                      </div>
                    {%- endif -%}
                    <div class="carousel__item--wrapper">
                    {% if block.settings.heading != blank %}
                      <h5 class="carousel__item--title" style="color: {{ block.settings.heading_color }};">
                        {% if block.settings.url != blank %}
                          <a href="{{ block.settings.url }}">
                        {% endif %}
                        {{ block.settings.heading }}
                        {% if block.settings.url != blank %}
                          </a>
                        {% endif %}
                      </h5>
                    {% endif %}
                    {% if block.settings.content != blank %}
                      <div class="carousel__item--description" style="color: {{ block.settings.text_color }};">
                        {{ block.settings.content }}
                      </div>
                    {% endif %}
                    {% if block.settings.button_text != blank or block.settings.button_url != blank %}
                      <div class="carousel__item--button">

                      <a href="{{ block.settings.button_url |  default: "#" }}" class="{{ block.settings.button_style }}" style="--btn-color: {{ block.settings.button_color }}; --btn-bg: {{ block.settings.button_bg_color }}; --btn-color-hover: {{ block.settings.button_color_hover }}; --btn-bg-hover: {{ block.settings.button_bg_color_hover }} ">
                        <span class="icon-button">{%- render 'icon-arrow' -%}</span>
                        {{ block.settings.button_text }}
                      </a>
                      </div>
                    {% endif %}
                    </div>
                  </div>
                </div>
            {%- endcase -%}
          </div>
        {% endfor %}
      </div>

    </div>
    {% if section.settings.navigation %}
      <div class="swiper-arrow">
        <div class="swiper-button-prev swiper-button-prev_{{ section.id }}">
          <svg
            width="11"
            height="10"
            viewBox="0 0 11 10"
            fill="none"
            xmlns="http://www.w3.org/2000/svg">
            <path d="M0.757812 5.34375L4.32031 9.09375C4.50781 9.28125 4.73438 9.375 5 9.375C5.25 9.375 5.46875 9.28906 5.65625 9.11719C5.84375 8.94531 5.9375 8.72656 5.9375 8.46094C5.9375 8.19531 5.85156 7.96875 5.67969 7.78125L3.61719 5.625H10.0625C10.3281 5.625 10.5547 5.53906 10.7422 5.36719C10.9141 5.17969 11 4.95312 11 4.6875C11 4.42188 10.9141 4.20312 10.7422 4.03125C10.5547 3.84375 10.3281 3.75 10.0625 3.75H3.61719L5.67969 1.59375C5.85156 1.40625 5.9375 1.17969 5.9375 0.914062C5.9375 0.648438 5.84375 0.429688 5.65625 0.257812C5.46875 0.0859375 5.24219 0 4.97656 0C4.71094 0 4.49219 0.09375 4.32031 0.28125L0.757812 4.03125C0.585938 4.21875 0.5 4.4375 0.5 4.6875C0.5 4.9375 0.585938 5.15625 0.757812 5.34375Z" fill="currentColor" />
          </svg>


        </div>
        <div class="swiper-button-next swiper-button-next_{{ section.id }}">
          <svg
            width="11"
            height="10"
            viewBox="0 0 11 10"
            fill="none"
            xmlns="http://www.w3.org/2000/svg">
            <path d="M10.2422 5.34375L6.67969 9.09375C6.49219 9.28125 6.26562 9.375 6 9.375C5.75 9.375 5.53125 9.28906 5.34375 9.11719C5.15625 8.94531 5.0625 8.72656 5.0625 8.46094C5.0625 8.19531 5.14844 7.96875 5.32031 7.78125L7.38281 5.625H0.9375C0.671875 5.625 0.445312 5.53906 0.257812 5.36719C0.0859375 5.17969 0 4.95312 0 4.6875C0 4.42188 0.0859375 4.20312 0.257812 4.03125C0.445312 3.84375 0.671875 3.75 0.9375 3.75H7.38281L5.32031 1.59375C5.14844 1.40625 5.0625 1.17969 5.0625 0.914062C5.0625 0.648438 5.15625 0.429688 5.34375 0.257812C5.53125 0.0859375 5.75781 0 6.02344 0C6.28906 0 6.50781 0.09375 6.67969 0.28125L10.2422 4.03125C10.4141 4.21875 10.5 4.4375 10.5 4.6875C10.5 4.9375 10.4141 5.15625 10.2422 5.34375Z" fill="currentColor" />
          </svg>

        </div>

      </div>
    {% endif %}

    {% if section.settings.pagination %}
      <div class="swiper-pagination swiper-pagination_{{ section.id }}"></div>
    {% endif %}

    {%- if section.settings.custom_html != blank -%}
      {{ section.settings.custom_html }}
    {%- endif -%}
  </div>
    
    


  
    </div>
</div>

{% schema %}
  {
    "name": "Blocks Carousel",
    "class": "shopify-section--blocks-carousel",
    "tag": "section",
    "settings": [
      {
      "type": "header",
      "content": "Section settings"
    },
    {
          "type": "checkbox",
          "id": "full_width",
          "label": "Full width",
          "default": false
        },
       {
        "type": "number",
        "id": "content_width",
        "label": "Content width",
        "default": 1290
       },
      {
        "type": "text",
        "id": "section_class",
        "label": "class"
      },
      {
        "type": "color",
        "id": "background_color",
        "label": "t:sections.all.background-color.label"
      },
      {
        "type": "image_picker",
        "id": "background_image",
        "label": "t:sections.all.background_image.label"
      },
      {
      "type": "checkbox",
      "id": "background_repeat",
      "label": "Background repeat",
      "default": false
    }, {
      "type": "select",
      "id": "background_size",
      "label": "Background size",
      "options": [
        {
          "value": "default",
          "label": "Default"
        }, {
          "value": "auto",
          "label": "Auto"
        }, {
          "value": "cover",
          "label": "Cover"
        }, {
          "value": "contain",
          "label": "Contain"
        }, {
          "value": "custom",
          "label": "Custom"
        }
      ],
      "default": "default"
    },
    {
      "type": "number",
      "id": "background_size_custom",
      "label": "Background size custom",
      "default": 100
    },
    {
      "type": "select",
      "id": "background_position",
      "label": "Background position",
      "options": [
        {
          "value": "center",
          "label": "Center"
        },
        {
          "value": "top left",
          "label": "Top Left"
        },
        {
          "value": "top center",
          "label": "Top Center"
        },
        {
          "value": "top right",
          "label": "Top right"
        },
        {
          "value": "center left",
          "label": "Center Left"
        }, {
          "value": "center right",
          "label": "Center Right"
        },
        {
          "value": "bottom left",
          "label": "Bottom Left"
        },
        {
          "value": "bottom center",
          "label": "Bottom Center"
        },
        {
          "value": "bottom right",
          "label": "Bottom Right"
        }, {
          "value": "custom",
          "label": "Custom"
        }
      ],
      "default": "center"
    },
    {
      "type": "text",
      "id": "background_position_x",
      "label": "Background position x"
    }, {
      "type": "text",
      "id": "background_position_y",
      "label": "Background position y"
    },
      {
        "type": "text",
        "id": "margin_desktop",
        "label": "t:sections.all.margin_desktop.label",
        "info": "t:sections.all.margin_desktop.info"
      }, {
        "type": "text",
        "id": "margin_tablet",
        "label": "t:sections.all.margin_tablet.label",
        "info": "t:sections.all.margin_tablet.info"
      }, {
        "type": "text",
        "id": "margin_mobile",
        "label": "t:sections.all.margin_mobile.label",
        "info": "t:sections.all.margin_mobile.info"
      }, {
        "type": "text",
        "id": "padding_desktop",
        "label": "t:sections.all.padding_desktop.label",
        "info": "t:sections.all.padding_desktop.info"
      }, {
        "type": "text",
        "id": "padding_tablet",
        "label": "t:sections.all.padding_tablet.label",
        "info": "t:sections.all.padding_tablet.info"
      }, {
        "type": "text",
        "id": "padding_mobile",
        "label": "t:sections.all.padding_mobile.label",
        "info": "t:sections.all.padding_mobile.info"
      }, {
          "type": "header",
          "content": "Title settings"
        },
        {
          "type": "liquid",
          "id": "title_content",
          "label": "Content"
        },
        {
          "type": "number",
          "id": "title_font_size",
          "label": "Font size",
          "default": 14
        },
        {
          "type": "number",
          "id": "title_line_height",
          "label": "Line height (px)",
          "default": 20
        },
        {
          "type": "select",
          "id": "title_font_weight",
          "label": "Font weight",
          "default": "normal",
          "options": [
            {
              "value": "normal",
              "label": "Normal"
            },
            {
              "value": "400",
              "label": "400"
            },
            {
              "value": "500",
              "label": "500"
            },
            {
              "value": "600",
              "label": "600"
            },
            {
              "value": "700",
              "label": "700"
            }
          ]
        },
        {
          "type": "select",
          "id": "title_text_transform",
          "label": "Text transform",
          "default": "none",
          "options": [
            {
              "value": "none",
              "label": "None"
            },
            {
              "value": "uppercase",
              "label": "Uppercase"
            },
            {
              "value": "lowercase",
              "label": "Lowercase"
            },
            {
              "value": "capitalize",
              "label": "Capitalize"
            }
          ]
        },
        {
          "type": "color",
          "id": "title_color",
          "label": "Color"
        },
        {
          "type": "number",
          "id": "title_space",
          "label": "Title space (px)",
          "default": 10
        },
        {
        "type": "number",
        "id": "title_space_tablet",
        "label": "Title space tablet (px)",
        "default": 30
      },
        {
          "type": "number",
          "id": "title_space_mobile",
          "label": "Title space mobile (px)",
          "default": 10
        },
        {
          "type": "header",
          "content": "Heading settings"
        },
        {
          "type": "image_picker",
          "id": "image_in_heading",
          "label": "Image in heading"
