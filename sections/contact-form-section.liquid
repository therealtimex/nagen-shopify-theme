{{ 'section-contact-form.css' | asset_url | stylesheet_tag }}
{% render 'section-style' %}
{%- assign id = '#shopify-section-' | append: section.id -%}

{%- style -%}
  {{ id }} {
    {%- if section.settings.background_repeat -%}
      background-repeat: repeat;
    {%- else -%}
      background-repeat: no-repeat;
    {%- endif -%}
    {%- if section.settings.background_size != "default" and section.settings.background_size != "custom" -%}
      background-size: {{ section.settings.background_size }};
    {%- endif -%}
    {%- if section.settings.background_size == "custom" -%}
      background-size: {{ section.settings.background_size_custom }}%;
    {%- endif -%}
    {%- if section.settings.background_position != "custom" -%}
      background-position: {{ section.settings.background_position }};
    {%- else -%}
      background-position-x: {{ section.settings.background_position_x }};
      background-position-y: {{ section.settings.background_position_y }};
    {%- endif -%}
  }

  {%- for block in section.blocks -%}
    #block-{{ block.id }} .image-with-text__image img {
      border-radius: {{ block.settings.image_rounded }};
    }

    #block-{{ block.id }}
    .image-with-text__heading {
      font-size: {{ block.settings.heading_font_size |  append: 'px' }};
      line-height: {{ block.settings.heading_line_height |  append: 'px' }};
      font-weight: {{ block.settings.heading_font_weight }};
      color: {{ block.settings.heading_color }};
      margin-bottom: {{ block.settings.heading_space |  append: 'px' }};
    }
    #block-{{ block.id }}
    .image-with-text__description {
      font-size: {{ block.settings.des_font_size |  append: 'px' }};
      line-height: {{ block.settings.des_line_height |  append: 'px' }};
      color: {{ block.settings.des_color }};
      margin-bottom: {{ block.settings.des_space |  append: 'px' }}
    }

    #block-{{ block.id }}
    .image-with-text {
      position: relative;
    }

    #block-{{ block.id }}
    .image-with-text__content {
      {%- if block.settings.content_position == "absolute" -%}
        position: absolute;
        inset: {{ block.settings.content_inset }};
      {%- endif -%}
      {%- if block.settings.content_max_width > 0 -%}
        max-width: {{ block.settings.content_max_width |  append: 'px' }};
      {%- endif -%}
      padding: {{ block.settings.content_padding }};
      border-radius: {{ block.settings.content_rounded |  append: 'px' }};
      background-color: {{ block.settings.bg_color }};
    }

    #block-{{ block.id }}
    .image-with-text__content .btn-primary {
      width: 100%;
    }

    #block-{{ block.id }}
    .image-with-text__content .btn-primary .icon-button {
      background-color: var(--btn-bg-hover);
      color: var(--btn-color);
    }

    #block-{{ block.id }}
    .image-with-text__content .btn-primary:hover .icon-button {
      background-color: var(--btn-bg)
    }



    #block-{{ block.id }}
    .btn-primary b {
      font-size: 16px;
      font-weight: 700
    }


    #block-{{ block.id }} .{{ block.type }} {
      {%- if block.settings.block_radius != blank -%}
        border-radius: {{ block.settings.block_radius }};
      {%- endif -%}
    }

    /*  form */

    #block-{{ block.id }} .title_block {
      font-size: {{ block.settings.title_font_size |  append: 'px' }};
      line-height: {{ block.settings.title_line_height |  append: 'px' }};
      color: {{ block.settings.title_color }};
      margin-bottom: {{ block.settings.title_space |  append: 'px' }};
      font-weight: {{ block.settings.title_font_weight }};
      display: flex;
      align-items: center;
      gap: 0 8px;

      {%- if block.settings.text_align != blank -%}
        justify-content: {{ block.settings.text_align }};
      {%- endif -%}
       
    }

   

    #block-{{ block.id }} .heading_block {
      font-size: {{ block.settings.heading_font_size |  append: 'px' }};
      line-height: {{ block.settings.heading_line_height }};
      color: {{ block.settings.heading_color }};
      margin-bottom: {{ block.settings.heading_space |  append: 'px' }};
      font-weight: {{ block.settings.heading_font_weight }};
    }


    #block-{{ block.id }} .text_description {
      font-size: {{ block.settings.description_font_size |  append: 'px' }};
      line-height: {{ block.settings.description_line_height |  append: 'px' }};
      color: {{ block.settings.description_color }};
      margin-bottom: {{ block.settings.description_space |  append: 'px' }};
      font-weight: {{ block.settings.description_font_weight }};
    }

    #block-{{ block.id }} .form_info {
      font-size: {{ block.settings.form_info_font_size |  append: 'px' }};
      line-height: {{ block.settings.form_info_line_height |  append: 'px' }};
      color: {{ block.settings.form_info_color }};
      margin-bottom: {{ block.settings.form_info_space |  append: 'px' }};
    }

    #block-{{ block.id }} .blocks-content {
      max-width: {{ block.settings.content_width |  append: 'px' }};
    }


    @media (max-width: 1199px) {
      #block-{{ block.id }} .image-with-text__content {
        {%- if block.settings.content_position == "absolute" -%}
          inset: {{ block.settings.content_inset_tablet }};
        {%- endif -%}
        padding: {{ block.settings.content_padding_tablet }};
      }
      #block-{{ block.id }} .heading_block {
        font-size: {{ block.settings.heading_font_size_tablet |  append: 'px' }};
      }
    }

    

    @media (max-width: 767px) {
      #block-{{ block.id }} .image-with-text__content {
        {%- if block.settings.content_position == "absolute" -%}
          inset: {{ block.settings.content_inset_mobile }};
        {%- endif -%}
        padding: {{ block.settings.content_padding_mobile }};
      }
      #block-{{ block.id }} .heading_block {
        font-size: {{ block.settings.heading_font_size_mobile |  append: 'px' }};
      }
    }

  {%- endfor -%}

{%- endstyle -%}


<div class="contact-form-section">
  <div
    
    class="shopify-container {% if  section.settings.no_gutters %} no_gutters{% endif %}">
    <div class="row {% if section.settings.no_gutters %}no-margin{% endif %} align-items-{{ section.settings.section_align }}">
      {% for block in section.blocks %}
        <div
          id="block-{{ block.id }}"
          class="col {% if section.settings.no_gutters %}no-padding{% endif %}"
          {% if block.settings.data_aos != 'none' %}
          data-aos="{{ block.settings.data_aos }}"
          {% endif %}>
          <div class="shopify-block {{ block.type }} d-flex {% if block.settings.block_align %}align-items-{{ block.settings.block_align }}{% endif %}">
            {%- case block.type -%}
              {%- when 'block-text' -%}
                <div class="blocks-content text-{{ block.settings.text_align }} w-100">
                  {%- if block.settings.block_title != blank -%}
                    <div class="title_block">
                      {{ block.settings.block_title }}
                    </div>
                  {%- endif -%}
                  {%- if block.settings.block_heading != blank -%}
                    <h2 class="heading_block">
                      {{ block.settings.block_heading }}
                    </h2>
                  {%- endif -%}
                  {%- if block.settings.block_description != blank -%}
                    <div class="text_description">
                      {{ block.settings.block_description }}
                    </div>
                  {%- endif -%}

                  {%- if block.settings.box_label_1 != blank or block.settings.box_label_2 != blank -%}
                    <div class="box-icons">
                      {%- if block.settings.box_label_1 != blank -%}
                        <div class="box-icon-item">
                          {%- if block.settings.box_icon_1 != blank -%}
                            <div class="box-icon-image">
                              {{ block.settings.box_icon_1 | image_url: width: block.settings.box_icon_1.width | image_tag }}
                            </div>
                          {%- endif -%}

                          <div class="box-icon-content">
                            <p class="box-icon-label" style="color: {{ block.settings.box_label_color }}; margin-bottom: {{ block.settings.box_label_space }}px;">
                              {{ block.settings.box_label_1 }}
                            </p>
                            {%- if block.settings.box_info_1 != blank -%}
                              <p class="box-icon-info" style="color: {{ block.settings.box_info_color }};">
                                {{ block.settings.box_info_1 }}
                              </p>
                            {%- endif -%}
                          </div>

                        </div>
                      {%- endif -%}
                      {%- if block.settings.box_label_2 != blank -%}
                        <div class="box-icon-item">
                          {%- if block.settings.box_icon_2 != blank -%}
                            <div class="box-icon-image">
                              {{ block.settings.box_icon_2 | image_url: width: block.settings.box_icon_2.width | image_tag }}
                            </div>
                          {%- endif -%}

                          <div class="box-icon-content">
                            <p class="box-icon-label" style="color: {{ block.settings.box_label_color }}; margin-bottom: {{ block.settings.box_label_space }}px;">
                              {{ block.settings.box_label_2 }}
                            </p>
                            {%- if block.settings.box_info_2 != blank -%}
                              <p class="box-icon-info" style="color: {{ block.settings.box_info_color }};">
                                {{ block.settings.box_info_2 }}
                              </p>
                            {%- endif -%}
                          </div>

                        </div>
                      {%- endif -%}
                    </div>
                  {%- endif -%}


                </div>
              {%- when 'block-form' -%}
                <div class="blocks-content">
                  {%- if block.settings.form_title != blank -%}
                    <div class="title_block">{{ block.settings.form_title }}</div>
                  {%- endif -%}
                  {%- if block.settings.form_heading != blank -%}
                    <div class="heading_block">{{ block.settings.form_heading }}</div>
                  {%- endif -%}

                  <div class="form">
                    {%- form 'contact'
                      , id: 'ContactForm'
                      , class: 'isolate' -%}
                      {%- if form.posted_successfully? -%}
                        <h2
                          class="form-status form-status-list form__message"
                          tabindex="-1"
                          autofocus>{% render 'icon-success' %} {{ 'templates.contact.form.post_success' | t }}</h2>
                      {%- elsif form.errors -%}
                        <div class="form__message">
                          <h2
                            class="form-status caption-large text-body"
                            role="alert"
                            tabindex="-1"
                            autofocus>{% render 'icon-error' %} {{ 'templates.contact.form.error_heading' | t }}</h2>
                        </div>
                        <ul class="form-status-list caption-large" role="list">
                          <li>
                            <a href="#ContactForm-email" class="link">
                              {{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}
                            </a>
                          </li>
                        </ul>
                      {%- endif -%}


                      <div class="contact__fields">

                        {%- if block.settings.first_name_field -%}
                          <div class="field">
                            <label class="field__label d-none" for="first-name">First name</label>
                            <input
                              type="text"
                              name="contact[first_name]"
                              id="first-name"
                              placeholder="First Name" />
                          </div>
                        {%- endif -%}

                        {%- if block.settings.last_name_field -%}
                          <div class="field">
                            <label class="field__label d-none" for="last-name">Last name</label>
                            <input
                              type="text"
                              name="contact[last_name]"
                              id="last-name"
                              placeholder="Last Name" />
                          </div>
                        {%- endif -%}

                      </div>


                      <div class="contact__fields">
                        {%- if block.settings.phone_field -%}
                          <div class="field">
                            <input
                              type="tel"
                              id="ContactForm-phone"
                              class="field__input"
                              autocomplete="tel"
                              name="contact[{{ 'templates.contact.form.phone' | t }}]"
                              pattern="[0-9\-]*"
                              value="{% if form.phone %}{{ form.phone }}{% elsif customer %}{{ customer.phone }}{% endif %}"
                              placeholder="{{ 'templates.contact.form.phone' | t }}">
                            <label class="field__label d-none" for="ContactForm-phone">{{ 'templates.contact.form.phone' | t }}</label>
                          </div>
                        {%- endif -%}

                        {%- if block.settings.email_field -%}
                          <div class="field field--with-error">
                            <input
                              autocomplete="email"
                              type="email"
                              id="ContactForm-email"
                              class="field__input"
                              name="contact[email]"
                              spellcheck="false"
                              autocapitalize="off"
                              value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}"
                              aria-required="true"
                              {% if form.errors contains 'email' %}
                              aria-invalid="true"
                              aria-describedby="ContactForm-email-error"
                              {% endif %}
                              placeholder="{{ 'templates.contact.form.email' | t }}">
                            <label class="field__label d-none" for="ContactForm-email">{{ 'templates.contact.form.email' | t }}
                              <span aria-hidden="true">*</span>
                            </label>
                            {%- if form.errors contains 'email' -%}
                              <small class="contact__field-error" id="ContactForm-email-error">
                                <span class="visually-hidden">{{ 'accessibility.error' | t }}</span>
                                <span class="form__message">{% render 'icon-error' %}{{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}</span>
                              </small>
                            {%- endif -%}
                          </div>
                        {%- endif -%}
                      </div>

                      {%- if block.settings.order_number_field -%}
                        <div class="field">
                          <label class="field__label d-none" for="order-number">Order Number</label>
                          <input
                            type="text"
                            name="contact[order_number]"
                            id="order-number"
                            placeholder="Order Number" />
                        </div>
                      {%- endif -%}


                      {%- if block.settings.message_field -%}
                        <div class="field">
                          <textarea
                            rows="6"
                            id="ContactForm-body"
                            class="text-area field__input"
                            name="contact[{{ 'templates.contact.form.comment_2' | t }}]"
                            placeholder="{{ 'templates.contact.form.comment_2' | t }}">
                            {{- form.body -}}
                          </textarea>
                          <label class="form__label field__label d-none" for="ContactForm-body">{{ 'templates.contact.form.comment_2' | t }}</label>
                        </div>
                      {%- endif -%}


                      {%- if block.settings.form_info != blank -%}
                        <p class="form_info">{{ block.settings.form_info }}</p>
                      {%- endif -%}

                      <div class="contact__button">
                        <button type="submit" class="{{ block.settings.button_style }}" style="--btn-color: {{ block.settings.button_color }}; --btn-bg: {{ block.settings.button_bg_color }}; --btn-color-hover: {{ block.settings.button_color_hover }}; --btn-bg-hover: {{ block.settings.button_bg_color_hover }};">
                          
                          <span class="icon-button">
                            {%- render 'icon-arrow' -%}
                          </span>
                          {{ block.settings.button_content }}
                        </button>
                      </div>


                    {%- endform -%}
                  </div>
                </div>
              {%- when 'block-image' -%}
                <div class="image-with-text">
                  <div class="image-with-text__image">
                    {%- if block.settings.image != blank -%}
                      {{ block.settings.image | image_url: width: block.settings.image.width | image_tag }}
                    {%- else -%}
                      {{ 'image' | placeholder_svg_tag }}
                    {%- endif -%}
                  </div>
                  {%- if block.settings.heading_content != blank or block.settings.des_content != blank or block.settings.button_content != blank -%}
                    <div class="image-with-text__content">
                      {%- if block.settings.heading_content != blank -%}
                        <h6 class="image-with-text__heading">
                          {{ block.settings.heading_content }}
                        </h6>
                      {%- endif -%}
                      {%- if block.settings.des_content != blank -%}
                        <div class="image-with-text__description">
                          {{ block.settings.des_content }}
                        </div>
                      {%- endif -%}
                      {%- if block.settings.button_content -%}
                        <a
                          href="{{ block.settings.button_url |  default: "#" }}"
                          class="{{ block.settings.button_style }}"
                          style="--btn-color: {{ block.settings.button_color }}; --btn-bg: {{ block.settings.button_bg_color }}; --btn-color-hover: {{ block.settings.button_color_hover }}; --btn-bg-hover: {{ block.settings.button_bg_color_hover }};">
                          <span class="icon-button">
                            

                            {%- if block.settings.icon_button != blank -%}
                              {{ block.settings.icon_button }}
                              {%- else -%}
                              {%- render 'icon-arrow' -%}
                            {%- endif -%}
                          </span>
                          {{ block.settings.button_content }}
                        </a>
                      {%- endif -%}
                    </div>
                  {%- endif -%}

                </div>


            {%- endcase -%}
          </div>


        </div>
      {% endfor %}
    </div>
  </div>
</div>


{% schema %}
  {
    "name": "Contact form section",
    "tag": "section",
    "class": "section",
    "settings": [
      {
        "type": "header",
        "content": "Section settings"
      },
      {
        "type": "checkbox",
        "id": "full_width",
        "label": "Full width",
        "default": false
      },
      {
        "type": "number",
        "id": "content_width",
        "label": "Content width",
        "default": 1290
      },
      {
        "type": "text",
        "id": "section_class",
        "label": "class"
      }, {
        "type": "checkbox",
        "id": "no_gutters",
        "label": "No gutters",
        "default": false
      }, {
        "type": "number",
        "id": "z_index",
        "label": "Z-index"
      }, {
        "type": "checkbox",
        "id": "overflow_visible",
        "label": "Overflow Visible",
        "default": true
      }, {
        "type": "color",
        "id": "background_color",
        "label": "t:sections.all.background-color.label"
      }, {
        "type": "image_picker",
        "id": "background_image",
        "label": "t:sections.all.background_image.label"
      }, {
        "type": "checkbox",
        "id": "background_repeat",
        "label": "Background repeat",
        "default": false
      }, {
        "type": "select",
        "id": "background_size",
        "label": "Background size",
        "options": [
          {
            "value": "default",
            "label": "Default"
          },
          {
            "value": "auto",
            "label": "Auto"
          },
          {
            "value": "cover",
            "label": "Cover"
          },
          {
            "value": "contain",
            "label": "Contain"
          }, {
            "value": "custom",
            "label": "Custom"
          }
        ],
        "default": "default"
      }, {
        "type": "number",
        "id": "background_size_custom",
        "label": "Background size custom",
        "default": 100
      }, {
        "type": "select",
        "id": "background_position",
        "label": "Background position",
        "options": [
          {
            "value": "center",
            "label": "Center"
          },
          {
            "value": "top left",
            "label": "Top Left"
          },
          {
            "value": "top center",
            "label": "Top Center"
          },
          {
            "value": "top right",
            "label": "Top right"
          }, {
            "value": "center left",
            "label": "Center Left"
          }, {
            "value": "center right",
            "label": "Center Right"
          }, {
            "value": "bottom left",
            "label": "Bottom Left"
          }, {
            "value": "bottom center",
            "label": "Bottom Center"
          }, {
            "value": "bottom right",
            "label": "Bottom Right"
          }, {
            "value": "custom",
            "label": "Custom"
          }
        ],
        "default": "center"
      }, {
        "type": "text",
        "id": "background_position_x",
        "label": "Background position x"
      }, {
        "type": "text",
        "id": "background_position_y",
        "label": "Background position y"
      }, {
        "type": "text",
        "id": "margin_desktop",
        "label": "t:sections.all.margin_desktop.label",
        "info": "t:sections.all.margin_desktop.info"
      }, {
        "type": "text",
        "id": "margin_tablet",
        "label": "t:sections.all.margin_tablet.label",
