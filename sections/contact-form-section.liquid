            "default": "400",
            "options": [
              {
                "value": "400",
                "label": "400"
              },
              {
                "value": "500",
                "label": "500"
              },
              {
                "value": "600",
                "label": "600"
              },
              {
                "value": "700",
                "label": "700"
              }
            ]
          }, {
            "type": "color",
            "id": "title_color",
            "label": "Form title color",
            "default": "#000000"
          }, {
            "type": "number",
