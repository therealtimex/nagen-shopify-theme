          }, {
            "type": "text",
            "id": "block_margin_tablet",
            "label": "Block Margin Tablet"
          }, {
            "type": "text",
            "id": "block_margin_mobile",
            "label": "Block Margin Mobile"
          }, {
            "type": "header",
            "content": "Block Padding"
          }, {
            "type": "text",
            "id": "block_padding_desktop",
