<style>
  #shopify-section-{{ section.id }} {
    position: relative;
  }

    #shopify-section-{{ section.id }} .footer-top {
        background-color: {{ section.settings.background }};
        background-image: url({{ section.settings.background_image | image_url: width: 2000 }});
        background-position: {{ section.settings.bg_align }};
    }
    #shopify-section-{{ section.id }} .footer-top_inner {
        padding: {{ section.settings.padding_desktop }};
        border-width: {{ section.settings.border_width }};
        border-style: solid;
        border-color: {{ section.settings.color_border }};
    }
    .info-content_title {
        margin: 0;
    }
    .footer-top .row {
        align-items: END;
        gap: 15px 0;
    }
    @media (max-width: 767px) {
      #shopify-section-{{ section.id }} .footer-top_inner {
        padding: {{ section.settings.padding_mobile }};
      }
      .footer-top_content.text-left.d-flex {
        flex-direction: column;
        gap: 15px 0;
      }
      .footer-top_content.text-left {
        text-align: center;
      }
      #shopify-section-{{ section.id }} ul.social-media {
        justify-content: center;
      }
    }

    #shopify-section-{{ section.id }} .social-media__link {
      width: 20px;
      height: 20px;
      color: {{ section.settings.color_des }};
    }
    #shopify-section-{{ section.id }} ul.social-media {
      gap: 20px;
    }
    #shopify-section-{{ section.id }} .social-media__item {
      box-shadow: none;
    }

    /* NEWSLETTER */
    .button-newsletter {
      height: var(--form-input-field-height);
    }
    .button-newsletter.sub-heading {
      padding: 0 37px;
    }
    #shopify-section-{{ section.id }} .input__label {
      left: 28px;
    }
    #shopify-section-{{ section.id }} .input__field {
      padding-left: 28px;
    }
    @media (max-width: 480px) {
      #shopify-section-{{ section.id }} .wp-newsletter-input.d-flex {
        flex-wrap: wrap;
        justify-content: center;
        gap: 15px 0;
      }
    }

</style>

